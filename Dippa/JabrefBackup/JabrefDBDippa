@ARTICLE{Anthony1992,
  author = {Anthony, O.D.},
  title = {Track maintenance planning on BAR},
  journal = {Railway Track and Structures},
  year = {1992},
  volume = {88},
  number = {3},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Railw Track Struct},
  abstract = {The Bangor & Aroostook Railroad (BAR) is one regional carrier that
	has embraced computer technology. Lean in size and number, with 320
	employees, BAR allocates resources through accurate and comprehensive
	planning. Over the past six years, the Personal Computer has become
	an important maintenance-planning tool. (BAR also has a mainframe
	computer with 26 remote terminals; 13 PCs are connected to the mainframe
	so data can be downloaded. BAR has 36 PCs, 30 of which are located
	in the headquarters building and linked by a local area network.
	The PCs are not exotic. The chief engineer's system is a 386, 16-mhz
	speed, 40MB hard disk with four megabytes of expanded memory. It
	includes a VGA monitor, clone Acer 1100 model, used with DOS 3.3,
	Lotus 2.2, HG 2.3 and Honeywell database software known as Total
	Information Management. This hardware package can be replaced today
	for less than $2,800. To entice employees to become computer literate
	and to purchase computers for use in their homes, BAR offered an
	incentive program through which the railroad paid a percentage of
	the purchase price of a PC system and financed the rest through payroll
	deduction without interest. Another element in the formula for successful
	m/w planning through the use of PCs is having an accurate and comprehensible
	database. Railroads are historically great keepers of information,
	and BAR is no exception.},
  coden = {RTSTA},
  correspondence_address1 = {Anthony, O.D.},
