@CONFERENCE{CronstrÃ¶m2010,
  author = {CronstrÃ¶m, S. and Svensson, T. and Renhorn, I.},
  title = {D8: An image capturing software for advanced applications including
	temporal synchronization of imaging sensors},
  year = {2010},
  volume = {7662},
  address = {Orlando, FL},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@2fa23c1f
