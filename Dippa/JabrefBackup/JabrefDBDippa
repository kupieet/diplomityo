@CONFERENCE{Holmes19983027,
  author = {Holmes, S. and Ziomek, C. and Adolphsen, C. and Akre, R. and Allison,
	S. and Clark, S. and Dean, T. and Fuller, R.W. and Gold, S. and Jobe,
	R.K. and Koontz, R. and Lavine, T.L. and Nantista, C. and Wilson,
	Z. and Young, A.},
  title = {Low-Level RF signal processing for the Next Linear Collider Test
	Accelerator},
  year = {1998},
  volume = {3},
  pages = {3027-3029},
  address = {Vancouver, BC, CAN},
  publisher = {Institute of Electrical and Electronics Engineers Inc., Piscataway,
	NJ, United States},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@2c094e86
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@17025600
	Through org.apache.xalan.xsltc.dom.DOMAdapter@1a5c25cd; Conference
	Code:55135},
  abbrev_source_title = {Proc IEEE Part Accel Conf},
  abstract = {In the X-band accelerator system for the Next Linear Collider Test
	Accelerator (NLCTA), the Low Level RF (LLRF) drive system must be
	very phase stable, but concurrently, be very phase agile. Phase agility
	is needed to make the Stanford Linear Energy Doubler (SLED) power
	multiplier systems work and to shape the RF waveforms to compensate
	beam loading in the accelerator sections. Similarly, precision fast
	phase and amplitude monitors are required to view, track, and feed
	back on RF signals at various locations throughout the system. The
	LLRF is composed of several subsystems: the RF Reference System generates
	and distributes a reference 11.424 GHz signal to all of the RF stations,
	the Signal Processing Chassis creates the RF waveforms with the appropriate
	phase modulation, and the Phase Detector Assembly measures the amplitude
	and phase of monitored RF signals. The LLRF is run via VXI instrumentation.
	These instruments are controlled using HP VEE graphical programming
	software. Programs have been developed to shape the RF waveform,
	calibrate the phase modulators and demodulators, and display the
	measured waveforms. This paper describes these and other components
	of the LLRF system.},
  affiliation = {Stanford Linear Accelerator Cent, Stanford, United States},
  coden = {PIACE},
  correspondence_address1 = {Holmes, S.; Stanford Linear Accelerator Cent, Stanford, United States},
  document_type = {Article},
  journal = {Proceedings of the IEEE Particle Accelerator Conference},
  keywords = {Computer graphics; Linear accelerators; Modems; Particle accelerator
	accessories; Particle beam dynamics; Phase modulation; Signal processing,
	Linear collider test accelerator, Colliding beam accelerators},
  language = {English},
  owner = {ekupiain},
  source = {Scopus},
  sponsors = {US Department of Energy; National Science Foundation; Office of Naval
