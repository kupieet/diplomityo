@CONFERENCE{Nagy2010192,
  author = {Nagy, A. and Njima, M. and Mkrtchyan, L.},
  title = {A Bayesian based method for agile software development release planning
	and project health monitoring},
  year = {2010},
  pages = {192-199},
  address = {Thessaloniki},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@6a79c2ea
