@CONFERENCE{LÃ¼er2008461,
  author = {LÃ¼er, C.},
  title = {Transition from a waterfall-based capstone course to an agile model},
  year = {2008},
  pages = {461-467},
  address = {Las Vegas, NV},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@17d715b9
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@29d330e9
	Through org.apache.xalan.xsltc.dom.DOMAdapter@68302672},
  abbrev_source_title = {Proc. Int. Conf. Front. Educ.: Comput. Sci. Comput. Eng., FECS},
  abstract = {We report on the successful process of transitioning a 4th-year university
	software engineering capstone course from a waterfall approach to
	an iterative, more agile model. The setting included external requirements
	on the structure of the course as well as ingrained expectations
	by stakeholders. We gradually introduced agile tools and methods
	into the existing syllabus while preserving the overall structure
	of the course. The selected practices included automated unit testing,
	iterations, wikis, and pair programming, among others. The success
	or failure of each technology, as well as the overall approach, was
	evaluated. We found that some, but not all, agile techniques proved
	effective in the given setting, and recommend that teachers in a
	similar situation consider a gradual approach to the introduction
	of new technologies.},
  affiliation = {Computer Science Department, Ball State University, Muncie IN 47306,
