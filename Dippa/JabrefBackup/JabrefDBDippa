@CONFERENCE{Greening2010,
  author = {Greening, D.R.},
  title = {Enterprise scrum: Scaling scrum to the executive level},
  year = {2010},
  address = {Koloa, Kauai, HI},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@6658a358
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@454c1ae6
	Through org.apache.xalan.xsltc.dom.DOMAdapter@77a1595; Conference
	Code:80065},
  __markedentry = {[ekupiain:]},
  abbrev_source_title = {Proc. Annu. Hawaii Int. Conf. Syst. Sci.},
  abstract = {Our company manages 25 software engineering teams across 6 products
	using a single top-down Enterprise Scrum. We know of no other company
	doing this, yet it provides extreme visibility and control at the
	CXO level. It promotes agile thinking enterprise-wide, driving non-engineering
	departments to adopt Scrum. We believe it is making us more profitable.
	We estimate effort in team months, run quarterly Sprints, assign
	whole teams to projects, meet in weekly stand-ups. We start, postpone
	or cancel whole projects. Within individual projects, we still use
	1-4 week Sprints and all the trappings of the classic Scrum process,
	including, in some cases, Scrum-of-Scrums. New challenges arise:
	Shared resource constraints suggest Kanban methods. Net Present Value
	can justify prioritization, but creates controversy. Moving teams
	between projects requires rapid programming environment setup. The
	process forces executives to justify decisions. We want simple improvement
	metrics, but they seem elusive. © 2010 IEEE.},
