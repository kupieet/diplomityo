@CONFERENCE{Kai200792,
  author = {Kai, U.S.},
  title = {IEEE 1588 on windows XP® powered measurement Devices - Mastering
	the trigger challenge},
  year = {2007},
  pages = {92-95},
  address = {Vienna},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@10a361dc
