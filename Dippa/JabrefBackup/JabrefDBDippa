@CONFERENCE{Johnson20041897,
  author = {Johnson, C.a and Shanmugam, R.a and Roberts, L.a and Zinkgraf, S.b
	and Young, M.b and Cameron, L.c and Flores, A.c },
  title = {Linking lean healthcare to six sigma: An emergency department case
	study},
  year = {2004},
  pages = {1897-1910},
  address = {Houston, TX},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@58bf9a2f
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@b23b12e
	Through org.apache.xalan.xsltc.dom.DOMAdapter@544548b8; Conference
	Code:66321},
  abbrev_source_title = {Annu. Conf. Exhibition},
  abstract = {Any healthcare organization embarking on a process improvement program
	must first answer some tough questions: How and where do we start?
	What tools should we use to define, measure, analyze, improve, and
	then control our hard won improvements? We had the opportunity to
	introduce and effect some commonly used industrial/process engineering
	tools in a 113-bed, acute-care, hospital emergency department located
	in central Texas. In an effort to become more efficient, effective,
	and adaptable at meeting community and business needs, the hospital
	has agreed to allow their Emergency Department (ED) to be the focus
	of a Six Sigma/Lean Process Improvement and Computer Simulation study.
	The goal of the project is to use Six Sigma methodology - specifically
	the Define, Measure, Analyze, Improve, and Control (DMAIC) improvement
	format - to reengineer key processes within the emergency department.
	Furthermore, the hospital wished to address the dissatisfaction of
	patients who encounter long waiting times in the ED process. Concurrently,
	the hospital would like to address the frequency and sources of mistakes;
	and improve upon the general quality level of care given to their
	customers. The efficacy of the improvements would be measured by
	the extent to which the Six Sigma methodology helped to improve the
	quality of care while reducing patients' length of stay (LOS) and
	process errors. At this point in the study, the work has progressed
	through the Define, Measure, and Analyze stages in the Six Sigma
	methodology. The key process inputs identified as "process improvement
	targets" are as follows - equipment and supplies, diagnostic results,
	the patient chart, materials, personnel, and doctor communication.
	Currently, the study is progressing into the Six Sigma Improvement
	stage. The simulation aspect of the study utilizes ProModel's MedModel
	software and is in the "as-is" model building stage.},
  affiliation = {Texas State University, San Marcos, TX 78666, United States; SBTI,
