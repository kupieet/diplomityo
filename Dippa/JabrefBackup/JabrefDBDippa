@ARTICLE{Nizami200722,
  author = {Nizami, K.},
  title = {Global software development and delivery},
  journal = {Dr. Dobb's Journal},
  year = {2007},
  volume = {32},
  pages = {22-28},
  number = {8},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Dr. Dobb's J.},
  abstract = {Global (software) development and delivery (GDD) can be successful
	by protecting the team aspect of software development. The foundation
	for maintaining team cohesion in GDD is a global process framework
	that provides the arrangement necessary to ensure that global teams
	work together efficiently. A process framework provides GDD projects
	with team cohesion and direction without sacrificing the flexibility
	necessary for agile development, and also provides teams with the
	ability to adopt methods and best practices that are visible, practical,
	and governed. The platform should be capable of uniting global development
	teams with process and must be capable of providing them with the
	ability to work locally while they collaborate globally. Agreeing
	and incorporating common and understood best practices across GDD
	teams is the best way to establish cohesion on implementation style.
	A GDD metrics and measurement approach can also benefit procurement
	of software services.},
  coden = {DDJTE},
  document_type = {Article},
  issn = {1044789X},
  language = {English},
  source = {Scopus},
  stage2status = {Exclude},
  url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-34547448133&partnerID=40&md5=abd677ae6ab7f01509e929710df89e56}
