@CONFERENCE{DeAssisSantosESilva2006393,
  author = {De Assis Santos E Silva, F. and De Azevedo, C.A.V.},
  title = {A new version of the assistant-statistical assistance software},
  year = {2006},
  pages = {393-396},
  address = {Orlando, FL},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@5a76a49
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@3232e185
	Through org.apache.xalan.xsltc.dom.DOMAdapter@2a73fc82; Conference
	Code:75024},
  abbrev_source_title = {Comp. Agric. Nat. Resour. - Proc. World Cong.},
  abstract = {The software Assistat-Statistical Assistance was developed with the
	objective of being accessible for students and researchers, regarding
	its cost as well as the easiness of use, not being necessary specific
	knowledge of computer programming. The new version of the software
	Assistat-Statistical Assistance (version 7.3) has an easy installation
	program and runs in Windows 98, Windows NT, Windows XP and other
	operating systems from the Windows family. It makes variance analysis
	(ANOVA) for data from experiments with the following design: entirely
	randomized, randomized blocks, Latin square, split-plots, split-split-plots
	and factorial, applying jointly the test "F" to verify effect of
	significance and the test of Tukey or the test "t" to compare means.
	In the cases of experiments in split-plots, split-split-plots and
	factorial, it applies the crossed Tukey test in the interaction means,
	when there is significant interaction effect. It also makes regression
	in the variance analysis, in the case of quantitative treatments.
	It still performs adjustment test or adherence of series by the method
	of Kolmogorov-Smirnov, test of the correlation coefficient, test
	of Mann-Whitney, test of Kruskal-Wallis and the following normality
	tests: Kolmogorov-Smirnov, CramÃ©r-von Mises, Anderson-Darling,
