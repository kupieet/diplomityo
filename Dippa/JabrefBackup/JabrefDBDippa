@ARTICLE{Parrish200476,
  author = {Parrish, A.a and Smith, R.a and Hale, D.b and Hale, J.b },
  title = {A field study of developer pairs: Productivity impacts and implications},
  journal = {IEEE Software},
  year = {2004},
  volume = {21},
  pages = {76-79},
  number = {5},
  note = {cited By (since 1996)19},
  abbrev_source_title = {IEEE Software},
  abstract = {Previous data on how assigning development tasks to programmers affects
	development effort were reexamined, isolating and analyzing the productivity
	of two-person teams who were developing a statewide time- accounting
	system. The analysis covered 48 modules developed by professional
	programmer pairs. For each module, the effect of concurrency on pair
	productivity was examined. Three factors, including, team size, concurrency,
	and productivity were isolated for this study. Findings indicate
	that two-person teams working independently are more productive than
	those working concurrently.},
  affiliation = {Department of Computer Science, University of Alabama, Tuscaloosa,
	AL 35487-0290, United States; Dept. of Info. Syst. Stat./Mgmt.Sci.,
