@ARTICLE{Gilman200532,
  author = {Gilman, W.R.},
  title = {Multisensor metrology boosts lean manufacturing},
  journal = {Quality},
  year = {2005},
  volume = {44},
  pages = {32-37},
  number = {13},
  note = {cited By (since 1996)2},
  abbrev_source_title = {Qual},
  abstract = {Multisensors metrology systems use two or more sensors to measure
	a part in a single setup enabling more measurements on a single machine.
	It takes up less space than several separate machines and reduces
	utility requirements and allows other manufacturing equipment to
	be brought more closely together. Designers and manufacturers are
	able to combine geometric, prismatic shapes with free-form contours,
	and have critical clearance holes or locating points, which enable
	the multisensors to provide measurements where other sensors are
	facing physical limitations. The best implementations of these sensors
	in software are to calibtrate all the sensors so that they can be
	used at any point in a measurement routine.},
  affiliation = {North American Sales for Optical Gaging Products Inc., Rochester,
	NY, United States},
  coden = {QULTD},
  correspondence_address1 = {Gilman, W.R.; North American Sales for Optical Gaging Products Inc.,
	Rochester, NY, United States},
  document_type = {Review},
  issn = {03609936},
  keywords = {Manufacturing equipment; Multisensor metrology systems, Computer software,
	Sensor data fusion},
  language = {English},
  owner = {ekupiain},
