@CONFERENCE{Read2008,
  author = {Read, R.W. and Rogerson, J.W. and Hochgreb, S.},
  title = {Relight imaging at low temperature, Low pressure conditions},
  year = {2008},
  address = {Reno, NV},
  note = {cited By (since 1996)2; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@38acfeef
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@659519fa
	Through org.apache.xalan.xsltc.dom.DOMAdapter@1fdca030; Conference
	Code:82092},
  abbrev_source_title = {AIAA Aerosp. Sci. Meet. Exhib.},
  abstract = {High-altitude relight inside a lean, direct-injection, gas turbine
	combustor is investigated experimentally by high-speed imaging. Ignition
	testing is conducted at two operating conditions: a 'safe' point
	at which relight is possible, and an 'unsafe' condition where ignition
	is precluded by the large air mass flow rate. Image processing software
	has been developed to analyze the motion and breakup of the imaged
	flame. At the safe condition, the developing flame kernels are typically
	swept downstream and towards the fuel-injector center-line, before
	moving upstream. The comparatively large gas-phase velocity at the
	unsafe condition causes rapid disintegration of the ignition kernel.
	At neither condition does the size of the spark kernel determine
	ignition outcome. Good agreement is demonstrated between the measured
	spark-centroid velocity and CFD predictions of gas-phase velocity
	below the igniter tip. Copyright © 2008 by Robert Read.},
  affiliation = {Cambridge University Engineering Department, Trumpington Street, Cambridge,
	CB2 1PZ, United Kingdom},
  art_number = {2008-0957},
  correspondence_address1 = {Read, R. W.; Cambridge University Engineering Department, Trumpington
	Street, Cambridge, CB2 1PZ, United Kingdom},
  document_type = {Conference Paper},
  isbn = {9781563479373},
  journal = {46th AIAA Aerospace Sciences Meeting and Exhibit},
