@CONFERENCE{Xu2005275,
  author = {Xu, M. and Salami, B. and Obimbo, C.},
  title = {How to protect personal information against keyloggers},
  year = {2005},
  editor = {Hamza M.H.},
  pages = {275-280},
  address = {Honolulu, HI},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@125d78fb
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@36b1b028
	Through org.apache.xalan.xsltc.dom.DOMAdapter@4ba42e25; Conference
	Code:66316},
  abbrev_source_title = {Proc. Int. Conf. Internet Multimedia Syst. Appl.},
  abstract = {As hackers can easily abuse Keyloggers to gather information about
	a person or an organization; Keyloggers are a serious threat to the
	privacy and security of computer users. In this paper, we provide
	a detailed analysis of the mechanisms of three main types of Keyloggers
	on Windows XP, 2000 and NT operating systems, namely kernel based
	Keyloggers, thread based Keyloggers and hook based Keyloggers. We
