@CONFERENCE{Koch200761,
  author = {Koch, R. and Pionteck, T. and Albrecht, C. and Maehle, E.},
  title = {A lightweight framework for runtime reconfigurable system prototyping},
  year = {2007},
  pages = {61-64},
  address = {Porto alegre},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@983a80f
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@228e75aa
	Through org.apache.xalan.xsltc.dom.DOMAdapter@36cefd9f; Conference
	Code:70234},
  abbrev_source_title = {Proc Int Workshop Rapid Syst Prototyping},
  abstract = {This paper describes a lightweight framework for prototyping runtime
	reconfigurable systems in a Xilinx Virtex-II Pro FPGA. The framework
	provides a reconfiguration and control infrastructure that allows
	components of the prototype system to be modified or exchanged at
	runtime by means of partial reconfiguration of the FPGA. The system
	state may be monitored and influenced by a programmable controller
	which is part of the framework. The area over-head of the framework
	is kept low by efficiently utilising the two hard-wired PowerPC processor
	cores inside the FPGA while avoiding the use of resource-intense
	bus structures. Specific lean input/output modules are used with
	the one processor core while the other implements a Xilinx UltraController-II
	based design. Â© 2007 IEEE.},
  affiliation = {Institute of Computer Engineering, University of Luebeck, 23538 Luebeck,
	Germany},
  art_number = {4228486},
  coden = {PWRPE},
  correspondence_address1 = {Koch, R.; Institute of Computer Engineering, University of Luebeck,
	23538 Luebeck, Germany; email: koch@iti.uni-luebeck.de},
  document_type = {Conference Paper},
  doi = {10.1109/RSP.2007.7},
  isbn = {0769528341; 9780769528342},
  issn = {10746005},
  journal = {Proceedings of the International Workshop on Rapid System Prototyping},
  keywords = {Program processors; Programmable logic controllers; Resource allocation;
	Software prototyping, Hard-wired PowerPC processors; UltraController-II
	based design, Field programmable gate arrays (FPGA)},
  language = {English},
  references = {Albrecht, C., Foag, J., Koch, R., Maehle, E., DynaCORE - a dynamically
