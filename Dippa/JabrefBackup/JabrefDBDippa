@CONFERENCE{Summers2008,
  author = {Summers, D.a and Chen, G.a and Reese, B.b and Hutchinson, T.b and
	Liesching, M.c and Ying, H.c and Dover, R.c },
  title = {Automated reticle inspection data analysis for wafer fabs},
  year = {2008},
  volume = {7122},
  address = {Monterey, CA},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@7a67b7a9
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@2f4af707
	Through org.apache.xalan.xsltc.dom.DOMAdapter@62a004d2; Conference
	Code:75957},
  abbrev_source_title = {Proc SPIE Int Soc Opt Eng},
  abstract = {To minimize potential wafer yield loss due to mask defects, most wafer
	fabs implement some form of reticle inspection system to monitor
	photomask quality in high-volume wafer manufacturing environments.
	Traditionally, experienced operators review reticle defects found
	by an inspection tool and then manually classify each defect as 'pass,
	warn, or fail' based on its size and location. However, in the event
	reticle defects are suspected of causing repeating wafer defects
	on a completed wafer, potential defects on all associated reticles
	must be manually searched on a layer-by-layer basis in an effort
	to identify the reticle responsible for the wafer yield loss. This
	'problem reticle' search process is a very tedious and time-consuming
	task and may cause extended manufacturing line-down situations. Often
	times, Process Engineers and other team members need to manually
	investigate several reticle inspection reports to determine if yield
	loss can be tied to a specific layer. Because of the very nature
	of this detailed work, calculation errors may occur resulting in
	an incorrect root cause analysis effort. These delays waste valuable
	resources that could be spent working on other more productive activities.
	This paper examines an automated software solution for converting
	KLA-Tencor reticle inspection defect maps into a format compatible
	with KLA-Tencor's Klarity Defect â„¢ data analysis database.
	The objective is to use the graphical charting capabilities of Klarity
	Defect to reveal a clearer understanding of defect trends for individual
	reticle layers or entire mask sets. Automated analysis features include
	reticle defect count trend analysis and potentially stacking reticle
	defect maps for signature analysis against wafer inspection defect
	data. Other possible benefits include optimizing reticle inspection
	sample plans in an effort to support "lean manufacturing" initiatives
	for wafer fabs. Â© 2008 SPIE.},
  affiliation = {Freescale-ATMC, Austin, TX, United States; KLA-Tencor Corporation,
	Austin, TX, United States; KLA-Tencor Corporation, San Jose, CA,
	United States},
  art_number = {71223F},
  author_keywords = {Analysis; Automated; Inspection; Klarity; Reticle; Wafer},
  coden = {PSISD},
  correspondence_address1 = {Summers, D.; Freescale-ATMC, Austin, TX, United States},
  document_type = {Conference Paper},
  doi = {10.1117/12.801524},
  issn = {0277786X},
  journal = {Proceedings of SPIE - The International Society for Optical Engineering},
