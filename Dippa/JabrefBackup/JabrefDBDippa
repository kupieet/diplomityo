@ARTICLE{Verhage200228,
  author = {Verhage, A.},
  title = {FlameBeat: Predicting combustion problems from pressure signals},
  journal = {Turbomachinery International},
  year = {2002},
  volume = {43},
  pages = {28-29},
  number = {2},
  note = {cited By (since 1996)1},
  abbrev_source_title = {Turbomach. Int.},
  abstract = {The introduction of lean premix combustion in gas turbines has made
	the flames more susceptible to instabilities. This can result in
	increased acoustics (humming and chugging) and gas turbine trips.
	KEMA has developed a software package, FlameBeat, to detect subtle
	changes in the pattern of acoustic flame emission. Signals from pressure
	transducers mounted directly on the combustors are continuously monitored
	to obtain pressure pulsation spectra. The software can also be used
	to detect cracks caused by thermal fatigue, problems with gas quality,
	burner malfunctioning and leaking gaskets. A list is given of companies
	that have installed FlameBeat.},
  coden = {TUIND},
  document_type = {Article},
  issn = {01494147},
  keywords = {Acoustics; Combustors; Computer software; Cracks; Gas turbines; Transducers,
	Pressure signals, Combustion, acoustic emission; combustor; computer
	program; flame; gas turbine},
  language = {English},
  owner = {ekupiain},
  source = {Scopus},
  stage2status = {Exclude},
  timestamp = {2013.11.18},
  url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0036505626&partnerID=40&md5=387cfc217e41c2ea9190ac899f1f5e7b}
}

