@CONFERENCE{Peck2003427,
  author = {Peck, M.A. and Miller, L. and Cavender, A.R. and Gonzalez, M. and
	Hintz, T.},
  title = {An airbearing-based testbed for momentum control systems and spacecraft
	line of sight},
  year = {2003},
  editor = {Sheeres D.J., Pittelkau M.E., Proulx R.J., Cangahuala L.A.},
  volume = {114 I},
  pages = {427-446},
  address = {Ponce},
  note = {cited By (since 1996)6; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@48ebe762
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@77a3f854
	Through org.apache.xalan.xsltc.dom.DOMAdapter@2e049029; Conference
	Code:62445},
  abbrev_source_title = {Adv Astronaut Sci},
  abstract = {The next generation of agile, precisely pointed space systems will
	demand novel approaches to attitude control and dynamics. The paradigm
	of ever stiffer, ever more massive designs is likely to give way
	to active, passive, and/or hybrid active/passive structural control
	of payloads with soft, well-damped bus-to-payload interfaces. Agility,
	often achieved through the use of control-moment gyros (CMGs), can
	also benefit from the highly damped, readily predictable dynamics
	characteristic of this new paradigm. Honeywell is developing a spacecraft
	attitude-dynamics testbed that is designed to assist in research,
	demonstration, and validation of hardware and software architectures
	for such spacecraft. The testbed offers high-agility slew and scan
	capability (via six CMGs) with structural control (via active vibration-isolation
	and payloadsteering platforms and discrete high-performance structural
	dampers). The combination of a line-of-sight metrology system capable
	of sub-microradian resolution and a large range-of-motion spherical
	airbearing offers a unique environment for testing innovative controls
	and dynamics concepts in spacecraft design.},
  affiliation = {Special Projects, Honeywell Space Systems, 19019 N. 59th Avenue, Glendale,
