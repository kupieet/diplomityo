@CONFERENCE{Zhang2010111,
  author = {Zhang, J.-M.},
  title = {The software development process methodology of resource-based access
	control},
  year = {2010},
  volume = {4},
  pages = {111-117},
  address = {Singapore},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@4cc9633c
