@ARTICLE{Scholte2010253,
  author = {Scholte, T.a and Kirda, E.b },
  title = {Achieving life-cycle compliance of service-oriented architectures:
	Open issues and challenges},
  journal = {Lecture Notes in Computer Science (including subseries Lecture Notes
	in Artificial Intelligence and Lecture Notes in Bioinformatics)},
  year = {2010},
  volume = {5939 LNCS},
  pages = {253-267},
