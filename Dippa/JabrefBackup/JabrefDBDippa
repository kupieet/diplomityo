@ARTICLE{Shelley200639,
  author = {Shelley, T.},
  title = {Lean design teams up with rapid},
  journal = {Eureka},
  year = {2006},
  volume = {26},
  pages = {39},
  number = {9},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Eureka},
  abstract = {Kent Business Link has organized a seminar on designing lean at Greenwich
	University in Chatham. David Lanckmans, sample manufacturing manager
	at Delphi Diesel Systems in Gillingham, Kent explained how the company
	improved its design and production engineering processes to significanly
	slash delivery time of sample diesel injection pumps. The design
	to manufacture process starts with three dimensional designing in
	SolidWorks. Some parts are rapid prototyped using the Fused Deposition
	Modeling facility at the University in Greenwich in Chatham. CAM
	instructions are prepared with Virtual Gibbs CAM while Vericut is
	used for post processing and verification of tool paths. Finally,
	the Seiki Systems Program Management Machine Monitoring software
	is used to supervise machining. For 2007, the target is complete
	sample manufacture in four weeks instead of five.},
  coden = {ERKAE},
  document_type = {Article},
  issn = {02612097},
