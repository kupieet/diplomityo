@CONFERENCE{Ayed201266,
  author = {Ayed, H. and Vanderose, B. and Habra, N.},
  title = {A metamodel-based approach for customizing and assessing agile methods},
  year = {2012},
  pages = {66-74},
  address = {Lisbon},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@69fae6b1
