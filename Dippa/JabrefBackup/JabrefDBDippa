@ARTICLE{Kurian2007157,
  author = {Kurian, T.J.a and Medapa, P.K.a and Raghu, G.S.b},
  title = {SIPL-agile estimator: A design approach to fuzzy-based software for
	estimating agility},
  journal = {International Journal of Agile Manufacturing},
  year = {2007},
  volume = {10},
  pages = {157-166},
  number = {2},
  note = {cited By (since 1996)0},
  __markedentry = {[ekupiain:]},
  abbrev_source_title = {Int. J. Agile Manuf.},
  abstract = {In today's competitive world, it is imperative for software industries
	to use agility in the software development process. A recently published
	scheme by one of the authors to use the fuzzy inference system for
	agility metrics, using the parameter Project Velocity as a measure
	of agility, had encouraging results. Project velocity is dependent
	on various parameters that are fuzzy variables. The domain and dependence
	of these fuzzy variables can be represented through membership functions
	and rules. The simulations carried out using Fuzzy Logic Tool Box
	of MATLAB showed that project velocity and its dependent variables
	give a good measure of agility. This paper describes the architecture
	and implementation aspects of software, so as to estimate the agility
	of a software process using fuzzy techniques. The fuzzy if-then rules
	and coefficient are represented in the form of matrix, and the implementation
	of mis software is explained using the state machine model. © 2007
	ISAM.},
