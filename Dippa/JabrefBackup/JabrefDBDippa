@ARTICLE{Parry200522,
  author = {Parry, G.},
  title = {Counting the cost},
  journal = {Manufacturing Engineer},
  year = {2005},
  volume = {84},
  pages = {22-25},
  number = {1},
  note = {cited By (since 1996)1},
  abbrev_source_title = {Manuf Engin},
  abstract = {The problems associated with Enterprise Resource Planning (ERP) implementations
	are discussed. ERP implementations cost twice as much as planned
	and are delivered late with far less functionality than promised
	and are costing jobs in UK manufacturing. Success is often measured
	relative to the organization's goals and expectations for the system,
	in the case of ERP. Visual control may act as an intermediate between
	those ERP systems that clash with lean practices, and translate push
	to lean pull methodology. The main benefits brought by ERP systems
	include visibility of information, customer sales analysis, all in
	one system, and an ability to standardize.},
  coden = {MNFEE},
  document_type = {Review},
