@CONFERENCE{Hill2009406,
  author = {Hill, J.H. and Turner, H.A. and Edmondson, J.R. and Schmidt, D.C.},
  title = {Unit testing non-functional concerns of component-based distributed
	systems},
  year = {2009},
  pages = {406-415},
  address = {Denver, CO},
  note = {cited By (since 1996)1; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@4316a282
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@3072737
	Through org.apache.xalan.xsltc.dom.DOMAdapter@4a9f8d6e; Conference
	Code:76415},
  abbrev_source_title = {Proc. - Int. Conf. Softw. Test., Verif., Validation, ICST},
  abstract = {Unit testing component-based distributed systems traditionally involves
	testing functional concerns of the application logic throughout the
	software lifecycle. In contrast, testing non-functional distributed
	system concerns (e.g., end-to-end response time, security, and reliability)
	typically does not occur until system integration because it requires
	a complete system to perform such tests, as well as sophisticated
	techniques to identify and analyze performance metrics that constitute
	non-functional concerns. Unit testing non-functional concerns is
	even harder in an agile development environment, due to the disconnect
	between high-level system specification and low-level performance
	metrics. This paper describes a methodology and tool called Understanding
	Non-functional Intentions via Testing and Experimentation (UNITE).
	UNITE is designed to unit test nonfunctional concerns of three component-based
	distributed systems. The results from applying UNITE to a componentbased
	distributed system show how it simplifies unit testing and evaluation
	of non-functional properties during the early stages of the software
	lifecycle.},
  affiliation = {Vanderbilt University, Nashville, TN, United States},
  art_number = {4815374},
  correspondence_address1 = {Hill, J. H.; Vanderbilt University, Nashville, TN, United States;
	email: j.hill@vanderbilt.edu},
  document_type = {Conference Paper},
  doi = {10.1109/ICST.2009.44},
  isbn = {9780769536019},
  journal = {Proceedings - 2nd International Conference on Software Testing, Verification,
	and Validation, ICST 2009},
