@ARTICLE{Hinderliter200822,
  author = {Hinderliter, H.},
  title = {On-press color control},
  journal = {Graphic Arts Monthly},
  year = {2008},
  volume = {80},
  pages = {22-23},
  number = {5},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Graph. Arts Mon.},
  abstract = {The closed-loop color systems, combining color measurement hardware
	and process control software to continually monitor and ink densities
	on press, are easy to justify even in lean times. The sheetfed presses
	are relative newcorners to closed-loop color, with most relying on
	near-line measurement of press sheets by a scanning spectrophotometer
	at the console. The color control pioneer Graphic Microsystems (GMI)
	have introduced Spectral Press Expert (SPX), a time, trend and artificial
	intelligence application provides real-time feedback on press conditions,
	but also collects data throughout the run, compiling trends and issuing
	warnings. GMI's measurement tools are often teamed with MAN Roland's
	Pecom press consoles, as the press management component of MAN's
	printnet networking and integration connects Pecom third-party scanners
	to both web and sheet-feds.},
