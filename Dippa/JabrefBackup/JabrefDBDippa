@ARTICLE{Mishra2012122,
  author = {Mishra, D. and Balcioglu, E. and Mishra, A.},
  title = {Measuring project and quality aspects in agile software development},
  journal = {Technics Technologies Education Management},
  year = {2012},
  volume = {7},
  pages = {122-127},
  number = {1},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Tech. Technol. Educ. Manage.},
  abstract = {Constructivism is a cognitive theory that emphasizes that learners
	construct new knowledge by rearranging and refining their existing
	knowledge. Agile software development methods follows this approach
	of learning as software is developed in short iterations and agile
	team incorporated all new knowledge gained through feedbacks from
	preceeding iterations. Agile software development methods are different
	from traditional software development methods and therefore, metrics
	proposed for projects using traditional software development methods
	are not fit to use in agile software development projects. In this
	paper, new metrics are proposed that can measure the progress and
	quality of the product in agile software development projects. The
	proposed metrics are also compared with existing agile metrics.},
  affiliation = {Department of Computer Engineering, Atilim University, Ankara, Turkey},
