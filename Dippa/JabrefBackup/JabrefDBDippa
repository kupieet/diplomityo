@ARTICLE{Moser2006287,
  author = {Moser, R.a and Sillitti, A.a and Abrahamsson, P.b and Succi, G.a
	},
  title = {Does refactoring improve reusability?},
  journal = {Lecture Notes in Computer Science (including subseries Lecture Notes
	in Artificial Intelligence and Lecture Notes in Bioinformatics)},
  year = {2006},
  volume = {4039 LNCS},
  pages = {287-297},
  note = {cited By (since 1996)8; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@6a316406
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@509dfc61
	Through org.apache.xalan.xsltc.dom.DOMAdapter@34246da0; Conference
	Code:67901},
  abbrev_source_title = {Lect. Notes Comput. Sci.},
  abstract = {The improvement of the software development process through the development
	and utilization of high quality and reusable software components
	has been advocated for a long time. Agile Methods promote some interesting
	practices, in particular the practice of refactoring, which are supposed
	to improve understandability and maintainability of source code.
	In this research we analyze if refactoring promotes ad-hoc reuse
	of object-oriented classes by improving internal quality metrics.
	We conduct a case study in a close-to industrial, agile environment
	in order to analyze the impact of refactoring on internal quality
	metrics of source code. Our findings sustain the hypothesis that
	refactoring enhances quality and reusability of - otherwise hard
	to reuse - classes in an agile development environment. Given such
	promising results, additional experimentation is required to validate
	and generalize the results of this work. Â© Springer-Verlag Berlin
	Heidelberg 2006.},
  address = {Turin},
  affiliation = {Center for Applied Software Engineering, Free University of Bolzano,
	Piazza Domenicani 3, Bozen, Italy; VTT Electronics, Oulu, Finland},
  correspondence_address1 = {Moser, R.; Center for Applied Software Engineering, Free University
	of Bolzano, Piazza Domenicani 3, Bozen, Italy; email: rmoser@unibz.it},
  document_type = {Conference Paper},
  doi = {10.1007/11763864_21},
