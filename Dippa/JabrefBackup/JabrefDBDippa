@CONFERENCE{Stettina2011159,
  author = {Stettina, C.J. and Heijstek, W.},
  title = {Necessary and neglected? An empirical study of internal documentation
	in agile software development teams},
  year = {2011},
  pages = {159-166},
  address = {Pisa},
  note = {cited By (since 1996)2; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@36e7243a
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@fb77ff Through
	org.apache.xalan.xsltc.dom.DOMAdapter@7d8306d9; Conference Code:86991},
  abbrev_source_title = {SIGDOC - Proc. ACM Int. Conf. Des. Commun.},
  abstract = {When compared to traditional development methods, agile development
	practices are associated with more direct communication and less
	documentation. However, few empirical studies exist that investigate
	the role of documentation in agile development teams. We thus employed
	a questionnaire to measure the perceptions of a group of agile practitioners
	with regard to the documentation in their projects. We obtained responses
	from 79 agile software development professionals and 8 teams in 13
	different countries. Our findings include that over half of developers
	in our data set find documentation important or even very important
	but that too little documentation is available in their projects.
	Agile practitioners do not seem to agree with the agile principle
	that "The most efficient and effective method of conveying information
	to and within a development team is face-to-face conversation." We
	were able to validate this result for a set of dissimilar agile teams
	in various domains. Â© 2011 ACM.},
  affiliation = {Leiden Institute of Advanced Computer Science, Leiden University,
	Niels Bohrweg 1, 2333 CA Leiden, Netherlands},
  author_keywords = {agile software development; documentation; empirical study},
  correspondence_address1 = {Stettina, C.J.; Leiden Institute of Advanced Computer Science, Leiden
	University, Niels Bohrweg 1, 2333 CA Leiden, Netherlands; email:
	stettina@liacs.nl},
  document_type = {Conference Paper},
  doi = {10.1145/2038476.2038509},
  isbn = {9781450309363},
  journal = {SIGDOC'11 - Proceedings of the 29th ACM International Conference
