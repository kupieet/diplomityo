@CONFERENCE{Short19941284,
  author = {Short, Al},
  title = {Object-oriented performance management},
  year = {1994},
  volume = {2},
  pages = {1284-1291},
  address = {Orlando, FL, USA},
  publisher = {CMG, Chicago, IL, United States},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@578e3944
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@3a51cc44
	Through org.apache.xalan.xsltc.dom.DOMAdapter@5039a245; Conference
	Code:42881},
  abbrev_source_title = {CMG Proc},
  abstract = {Prior to the explosion of distributed systems, performance management
	used to be easy (sic) in that we typically were dealing with well
	defined systems and applications. The comparative robustness of the
	host-centric measurement tools provided the performance analyst and
	capacity planner with a wide range of options in managing current
	performance and planning for the future. Today, we find ourselves
	being charged with managing a diverse set of computing platforms,
	technologies, vendors, and applications which require us to be able
	to respond in both an agile and accurate manner. This paper describes
	how the application of Object-Oriented Analysis techniques can be
	borrowed from the software development paradigm and applied to Performance
	Management. This methodology, Object-Oriented Performance Management
	(OOPM) lends itself well to the management of distributed systems
	without leaving the host-centric paradigms behind.},
  affiliation = {LEGENT Corp},
  coden = {CMPRE},
  correspondence_address1 = {Short, Al; LEGENT Corp},
