@ARTICLE{DeWitt200641,
  author = {DeWitt, A.J.a b and Kuljis, J.a b c },
  title = {Is usable security an oxymoron?},
  journal = {Interactions},
  year = {2006},
  volume = {13},
  pages = {41-44},
  number = {3},
  note = {cited By (since 1996)4},
  abbrev_source_title = {Interactions},
  abstract = {The importance and the applicability of software security among the
	computer users, is discussed. Polaris has conducted a usability study
	of Polaris, an effective alpha release for Windows XP, and developed
	a system to make MS Windows safer from viruses and malicious codes.
	Principles such as Principle Of Least Authority (POLA) are one possible
	step towards aligning security and usability. Three different categories
	of usability metrics such as, effectiveness, efficiency and satisfaction,
	are used to assess the software, based on the definition of usability
	as given in ISO standard 9241-11. Users should be aware of the risks
	of using the Internet and can readily judge the trustworthiness of
	the web sites and e-mail, based on their experience.},
  affiliation = {Brunel University, United Kingdom; Brunel University, West London,
	United Kingdom; VIVID Research Centre, Brunel University, United
	Kingdom},
