@ARTICLE{Clarke200740,
  author = {Clarke, C.},
  title = {Plan ahead},
  journal = {Engineer},
  year = {2007},
  volume = {293},
  pages = {40-42},
  number = {7737},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Engineer},
  abstract = {A survey, conducted by the Aberdeen Group, on the interaction between
	lean and IT revealed that the choosing correct IT solutions is crucial
	to the successful implementation of lean manufacturing. Manufacturer
	can cut their cost twice by adopting lean scheduling software applications
	and integrating them with enterprise resource planning (ERP) systems.
	The outcome of such approach is to provide genuine supply chain visibility,
	which is a tool that empowers lean decision makers. The survey suggested
	that these decision makers can measure the benefits of lean manufacturing
	implementation by looking at delivery time and connection of demand
	with the execution with concern to the level of quality. The recommendations
	of the survey include, standardized lean scheduling and practices
	across the organization, based on findings of a continuous improvement
	team, decision makers must be allowed to make effective judgments,
	and lean software applications are to be adopted and integrated with
	ERP.},
  coden = {ENGIA},
