@CONFERENCE{Lui200494,
  author = {Lui, K.M. and Chan, K.C.C.},
  title = {A cognitive model for solo programming and pair programming},
  year = {2004},
  editor = {Chan C., Kinsner W., Wang Y., Miller D.M.},
  pages = {94-102},
  address = {Victoria, BC},
  note = {cited By (since 1996)6; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@1f4ffe04
