@CONFERENCE{SamsonJr.1996315,
  author = {Samson Jr., John R. and Grimm, David and Morrill, Kevin and Andresen,
	Ted},
  title = {STAP performance on a ParagonTM/Touchstone system},
  year = {1996},
  editor = {Anon},
  pages = {315-320},
  address = {Ann Arbor, MI, USA},
  publisher = {IEEE, Piscataway, NJ, United States},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@244e42d6
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@e060239
	Through org.apache.xalan.xsltc.dom.DOMAdapter@2c7bf360; Conference
	Code:45075},
  abbrev_source_title = {IEEE Nat Radar Conf Proc},
  abstract = {Honeywell's Embedded Touchstone and Rugged Touchstone systems are
	COTS-based, hardware equivalent and software compatible manifestations
	of the Intel Paragon XP/S Supercomputing System. The ParagonTM/Touchstone
	system is a robust, massively parallel processing architecture capable
	of supporting a wide range of implementation paradigms for DoD, DoE,
	and NASA applications. All applications, but airborne and space-based
	applications in particular, can benefit from the highest possible
	execution efficiency because execution efficiency translates into
	fewer nodes and correspondingly lower size, weight, power, and cost.
	This paper describes the performance obtained from the implementation
	of several STAP (Space-Time Adaptive Processing) benchmarks and the
	Real-Time Multi-Channel Airborne Radar Measurements (RTMCARM) application
	on ParagonTM/Touchstone processing systems using a high level, data
	parallel, data flow approach. Demonstrated performance is discussed
	in terms of end-to-end latency, speedup, high sustained throughput,
	high sustained implementation efficiency, high compute resource utilization
	efficiency, scalability, software portability and the software productivity
	improvement using Honeywell's Configurator and Data Flow Shell tools.},
  affiliation = {Honeywell Inc, Clearwater, United States},
  coden = {RINRF},
  correspondence_address1 = {Samson Jr., John R.; Honeywell Inc, Clearwater, United States},
  document_type = {Conference Paper},
  journal = {IEEE National Radar Conference - Proceedings},
  keywords = {Adaptive filtering; Data handling; Digital signal processing; Parallel
	processing systems; Supercomputers, Intel Paragon XP/S supercomputing
	system; Real time multichannel airborne radar measurements (RTMCARM);
	Space time adaptive processing (STAP); Touchstone processing systems,
	Radar systems},
  language = {English},
  owner = {ekupiain},
