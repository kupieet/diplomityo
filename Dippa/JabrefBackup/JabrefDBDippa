@CONFERENCE{Janes2009889,
  author = {Janes, A. and Succi, G.},
  title = {To pull or not to pull},
  year = {2009},
  pages = {889-894},
  address = {Orlando, FL},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@4ad53924
