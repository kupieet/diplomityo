@CONFERENCE{Liu20052378,
  author = {Liu, T. and Boumaiza, S. and Ghannouchi, F.M.},
  title = {Application of neural networks to 3 G power amplifier modeling},
  year = {2005},
  volume = {4},
  pages = {2378-2382},
  address = {Montreal, QC},
  note = {cited By (since 1996)3; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@530db042
