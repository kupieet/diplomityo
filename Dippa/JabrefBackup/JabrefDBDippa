@ARTICLE{Anon199572,
  author = {Anon},
  title = {Making virtual LANs a virtual snap},
  journal = {Data Communications},
  year = {1995},
  volume = {24},
  pages = {72-74},
  number = {1},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Data Commun},
  abstract = {Virtual LANs can present net managers with the stressful mouse-induced
	pointing and clicking because most LAN switches and routers that
	come with virtual LAN facilities require virtual workgroups to be
	set up and administered manually from a management console. To help
	prevent this problem, Agile Networks Inc (Concord, MA) has developed
	the ATMizer 125 Relational Switch by automatically allocating users
	to virtual LANs based on two criteria: network protocol and subnet
	address. The Ethernet/ATM switch owes its virtual LAN edge to a unique
	design that combines the self-learning abilities of a MAC-layer bridge
	with a router's ability to read protocols. This combination enables
	the ATMizer 125 to inspect network traffic and automatically group
	those workstations using the same protocol and having the same subnetwork
	address into the same virtual LAN. As network nodes are moved, added,
	or changed, the Agile switch reassigns them to the appropriate virtual
	LAN.},
  coden = {DACOD},
  document_type = {Article},
  issn = {03636399},
  keywords = {Asynchronous transfer mode; Computer software; Computer workstations;
	Data communication systems; Monitoring; Network protocols; Packet
	switching; Switching networks; Telecommunication traffic; User interfaces;
	Virtual reality, Internetworking; Virtual LAN; Virtual snap, Local
	area networks},
  language = {English},
  publisher = {McGraw-Hill Inc, New York, NY, United States},
  source = {Scopus},
  stage2status = {Exclude},
  url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0029235143&partnerID=40&md5=8f5df00bfc2773afd21961b4f8e76f66}
