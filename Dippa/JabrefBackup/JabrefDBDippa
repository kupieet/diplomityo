@CONFERENCE{DaÌ§browski2012208,
  author = {DaÌ§browski, R. and Stencel, K. and Timoszuk, G.},
  title = {Improving software quality by improving architecture management},
  year = {2012},
  pages = {208-215},
  address = {Ruse},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@4c4a1416
