@ARTICLE{Farahneh2011504,
  author = {Farahneh, H.O.a and Issa, A.A.b },
  title = {A Linear Use Case Based Software Cost Estimation Model},
  journal = {World Academy of Science, Engineering and Technology},
  year = {2011},
  volume = {73},
  pages = {504-508},
  note = {cited By (since 1996)1},
  abbrev_source_title = {World Acad. Sci. Eng. Technol.},
  abstract = {Software development is moving towards agility with use cases and
	scenarios being used for requirements stories. Estimates of software
	costs are becoming even more important than before as effects of
	delays is much larger in successive short releases context of agile
	development. Thus, this paper reports on the development of new linear
	use case based software cost estimation model applicable in the very
	early stages of software development being based on simple metric.
	Evaluation showed that accuracy of estimates varies between 43% and
	55% of actual effort of historical test projects. These results outperformed
	those of wellknown models when applied in the same context. Further
	work is being carried out to improve the performance of the proposed
	model when considering the effect of non-functional requirements.},
  affiliation = {Electrical Engineering Department, Faculty of Engineering and Technology
	University of Jordan Amman Jordan, Jordan; Software Engineering Department,
	Faculty of Information Technology, Philadelphia University, P.O.
	Box 1, Amman. 19392, Jordan},
