@CONFERENCE{Shawky2010230,
  author = {Shawky, D.M.a and Ali, A.F.b },
  title = {A practical measure for the agility of software development processes},
  year = {2010},
  pages = {230-234},
  address = {Cairo},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@53395960
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@5a1ad84f
	Through org.apache.xalan.xsltc.dom.DOMAdapter@186f58c0; Conference
	Code:83133},
  abbrev_source_title = {ICCTD - Int. Conf. Comput. Technol. Dev., Proc.},
  abstract = {In the software industry, a large number of projects fail and billions
	of dollars are spent on failed software projects. Lack of an end
	user involvement, poor requirements, and unrealistic schedules are
	some of the top reasons of such failure. Agile software development
	is an approach that addresses these problems through a real communication
	between programmers and customers. Thus, there is a need to quantify
	software agility. In this paper, an approach for quantitying software
	agility is provided by modeling the key concepts in agile software
	and proposing a measure that can be used in representing how agile
	a software development process is. The proposed measure employs information
	entropy as the main concept related to software agility. The suggested
	measure is tested on two open source case studies. Experimental results
	demonstrate the validity and suitability of the agility measure.
	Â© 2010 IEEE.},
  affiliation = {Engineering Mathematics Dept., Faculty of Engineering, Cairo University,
	Giza, Egypt; Biomedical Engineering Dept., Faculty of Engineering,
