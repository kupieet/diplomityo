@ARTICLE{Gilb200535,
  author = {Gilb, T.},
  title = {Agile specification quality control},
  journal = {Cutter IT Journal},
  year = {2005},
  volume = {18},
  pages = {35-39},
  number = {1},
  note = {cited By (since 1996)3},
  abbrev_source_title = {Cutter IT J.},
  abstract = {Various aspects of Agile Specification Quality Control (SQC) are discussed.
	The main purpose of Agile Specification Quality Control (SQC) is
	to motivate individuals to reduce major specification defect insertion.
	Secondary purposes include: prevention of uneconomic major defect
	density specs from escaping downstream and to teach and reinforce
	current specification standards. The key SQC concept compared to
	traditional software inspection methods is to measure by sampling
	and use the information to motivate people to learn the rules and
	reduce their defect injection rate.},
  document_type = {Review},
  issn = {15227383},
