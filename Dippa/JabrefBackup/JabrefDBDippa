@CONFERENCE{Smits2011274,
  author = {Smits, H.a and Rilliet, K.b },
  title = {Agile experience report: Transition \& complexity at Cisco Voice
	Technology Group},
  year = {2011},
  pages = {274-278},
  address = {Salt Lake City, UT},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@52fde238
