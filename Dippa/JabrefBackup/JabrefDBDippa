@CONFERENCE{Clifton2006,
  author = {Clifton, J.a b },
  title = {Software engineering practices used for retention?},
  year = {2006},
  address = {Chicago, IL},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@730e5b65
