@ARTICLE{Bailey2003485,
  author = {Bailey, M.B.a and Kreider, J.F.b },
  title = {Creating an automated chiller fault detection and diagnostics tool
	using a data fault library},
  journal = {ISA Transactions},
  year = {2003},
  volume = {42},
  pages = {485-495},
  number = {3},
  note = {cited By (since 1996)6},
  abbrev_source_title = {ISA Trans},
  abstract = {Reliable, automated detection and diagnosis of abnormal behavior within
	vapor compression refrigeration cycle (VCRC) equipment is extremely
	desirable for equipment owners and operators. The specific type of
	VCRC equipment studied in this paper is a 70-ton helical rotary,
	air-cooled chiller. The fault detection and diagnostic (FDD) tool
	developed as part of this research analyzes chiller operating data
	and detects faults through recognizing trends or patterns existing
	within the data [1]. The FDD method incorporates a neural network
	(NN) classifier to infer the current state given a vector of observables.
	Therefore the FDD method relies upon the availability of normal and
	fault empirical data for training purposes and therefore a fault
	library of empirical data is assembled [2]. This paper presents procedures
	for conducting sophisticated fault experiments on chillers that simulate
	air-cooled condenser, refrigerant, and oil related faults. The experimental
	processes described here are not well documented in literature and
	therefore will provide the interested reader with a useful guide.
	In addition, the authors provide evidence, based on both thermodynamics
	and empirical data analysis, that chiller performance is significantly
	degraded during fault operation. The chiller's performance degradation
	is successfully detected and classified by the NN FDD classifier
	as discussed in the paper's final section.},
  affiliation = {Dept. of Civil and Mechanical Eng., United States Military Academy,
	West Point, NY 10996-1792, United States; Joint Center for Energy
	Management, Dept of Civ. Environ. and Arch. Eng., University of Colorado,
	Campus Box 428, Boulder, CO 80309, United States},
  author_keywords = {DDC; Experimental research; Software},
  coden = {ISATA},
  correspondence_address1 = {Bailey, M.B.; Dept. of Civil and Mechanical Eng., United States Military
	Academy, West Point, NY 10996-1792, United States; email: Margaret.Bailey@usma.edu},
  document_type = {Article},
  issn = {00190578},
  keywords = {Automation; Neural networks; Observability; Refrigerants; Refrigeration;
	Vectors, Chiller; Data fault library; Fault detection and diagnostic
	tool; Vapor compression refrigeration cycle, Computer aided software
	engineering},
  language = {English},
  owner = {ekupiain},
  references = {Bailey, M., The design and viability of a probabilistic fault detection
	and diagnosis method for vapor compression cycle equipment (1998),
	Ph.D. dissertation, Department of Civil and Architectural Engineering,
	University of ColoradoMassie, D., Bailey, M., Optimization and fault
	evaluation of a building's cooling plant using neural networks Proceedings
	from ECOS 2000 International Conference on Efficiency, Cost, Optimization,
	Simulation and Environmental Aspects of Energy and Process Systems,
