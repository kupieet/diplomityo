@CONFERENCE{Wei20121251,
  author = {Wei, G. and Wang, Q.},
  title = {The application research on graphic traverse algorithm in graphic
	topology analysis},
  year = {2012},
  pages = {1251-1253},
  address = {Xi'an},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@748ff740
