@CONFERENCE{Nagarajan2003453,
  author = {Nagarajan, S.V. and Garcia, O. and Croll, P.R.},
  title = {Extreme testing practice in extreme programming (XP)},
  year = {2003},
  editor = {Hamza M.H.},
  volume = {7},
