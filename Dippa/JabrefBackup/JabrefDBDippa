@ARTICLE{NoAuthor200556,
  title = {Product quality is looking up, thanks to PLM adoption},
  journal = {Manufacturing Business Technology},
  year = {2005},
  volume = {23},
  pages = {56},
  number = {7},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Manuf. Bus. Technol.},
  abstract = {The implementation of product life cycle management (PLM) software
	for achieving time and cost savings as well as product quality is
	discussed. The improvement in product quality must be measured and
	quantified. Agile Software's Product Service and Improvement solution
	measures achieves these improvement in two distinct ways. First,
	it provides a means of capturing and integrating product-related
	quality data from across the value chain. Second, it allows prioritizing
	and driving improvement projects by analyzing the data and then integrating
	it with analyses of other identified faults, and the corrective actions
	that were taken to remedy them.},
