@CONFERENCE{Card1987145,
  author = {Card, D.N. and Cotnoir, D.V. and Goorevich, C.E.},
  title = {MANAGING SOFTWARE MAINTENANCE COST AND QUALITY.},
  year = {1987},
  pages = {145-152},
  address = {Austin, TX, USA},
  publisher = {IEEE, New York, NY, USA},
  note = {cited By (since 1996)3; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@1db7c9ba
	; Conference Code:10697},
  __markedentry = {[ekupiain:]},
  abstract = {The authors describe how measurements, estimation, and management
	interact to control software cost and quality in a large, long-lived
	maintenance project. Measures of effort expended and work performed
	from past releases assist managers in developing the accurate cost
	estimates necessary for effective planning. The tracking of error
	reports allows managers to monitor quality trends and develop corrective
	actions when problems occur. The cost-estimation and quality-tracking
	procedures described support an established management structure
	based on a formal software development methodology called DSDM. It
	incorporates well-defined project control and product assurance functions
	that facilitate on-time delivery of reliable software.},
