@CONFERENCE{Ju2008107,
  author = {Ju, X. and Zou, H.},
  title = {Operating system robustness forecast and selection},
  year = {2008},
  pages = {107-116},
  address = {Seattle, WA},
  note = {cited By (since 1996)4; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@82cc556
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@334f954b
	Through org.apache.xalan.xsltc.dom.DOMAdapter@2dd2893e; Conference
	Code:77488},
  abbrev_source_title = {Proc. Int. Symp. Softw. Reliab. Eng.},
  abstract = {While commercial Off-the-Shelf (COTS) operating systems (OS) have
	long been widely used, the issue regarding their robustness is far
	from being solved. Although many efforts have been made in this research
	domain, people still find it difficult to make choices among various
	OSs for robustness concerns. This paper proposes a reference model
	for OS robustness forecast and selection that aims to forecast the
	robustness of specific OSs under given operational profiles. At the
	same time, the model can select appropriate OSs as development/operating
	platforms that meet the particular robustness requirements of the
	target workloads. Our model combines OSs' overall robustness with
	operational profiles and uses extensive tests on OS APIs to make
	our calculation. We have measured 255 APIs and CLibrary functions
	on Windows XP and Vista, and 197 CLibrary functions on Linux 2.6.22
	(Ubuntu 7.10). Our results show that on average Windows XP and Vista
	are more robust than Linux, but their performances are comparable
	under compute-intensive workloads. A demonstration of how these results
	are used in the proposed reference model for OSs robustness forecast
	and selection is given at the end. Â© 2008 IEEE.},
  affiliation = {School of Software and Department of Computer Science, Shanghai Jiao
	Tong University, Shanghai 200240, China},
