@CONFERENCE{Drelia20111494,
  author = {Drelia, M.G.a and Paciello, V.b },
  title = {Performance evaluation of LabView on Linux Ubuntu and Window XP operating
	systems},
  year = {2011},
  pages = {1494-1498},
  address = {Belgrade},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@50caf79c
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@3d464c34
	Through org.apache.xalan.xsltc.dom.DOMAdapter@52350959; Conference
	Code:88680},
  abbrev_source_title = {Telecommun. Forum, TELFOR - Proc. Pap.},
  abstract = {This paper presents a study on implementation and performance evaluations
	on data application measurements in two different operating Systems:
	Ubuntu 8.4 and Windows Xp. After a description of the system, a comparison
	of the performance of digital signal acquisition and processing on
	linux and windows using LabView software, is reported. The experimental
	results show that the system can work stably and effectively on both
