@ARTICLE{Denning200924,
  author = {Denning, P.J.a and Riehle, R.D.b },
  title = {The profession of IT: Is software engineering engineering?},
  journal = {Communications of the ACM},
  year = {2009},
  volume = {52},
  pages = {24-26},
  number = {3},
  note = {cited By (since 1996)13},
  abbrev_source_title = {Commun ACM},
  abstract = {Software engineering has contributed principles for managing complexity
	in software systems, delivering reliable, dependable, and affordable
	software. In software engineering, the process models have evolved
	into various forms that range from highly structured programming
	to unstructured agile processes. Specifications from well-researched
	requirements are derived and tools from program verification and
	process management are used to meet the specifications. Design metrics,
	including design to tolerance involves allowable stresses, tolerances
	performance ranges, structural complexity, and failure probabilities
	for various conditions. Reconciliation of conflicting forces and
	constraints include arrays of non-technical economic, statutory societal,
	and logical constraints. Platform independence for software systems
	allows software to work under a choice of operating systems and platform-dependent
	functions gathered into a platform interface module.},
  affiliation = {Cebrowski Institute for Information Innovation and Superiority, Naval
