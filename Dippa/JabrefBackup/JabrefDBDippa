@CONFERENCE{Bareiss201065,
  author = {Bareiss, R. and Mercier, G.},
  title = {A graduate education in software management and the software business
	for mid-career professionals},
  year = {2010},
  pages = {65-72},
  address = {Pittsburgh, PA},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@1d39fe50
