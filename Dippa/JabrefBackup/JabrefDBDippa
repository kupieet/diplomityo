@ARTICLE{Madeyski2006278,
  author = {Madeyski, L.},
  title = {The impact of pair programming and test-driven development on package
	dependencies in object-oriented design - An experiment},
  journal = {Lecture Notes in Computer Science (including subseries Lecture Notes
	in Artificial Intelligence and Lecture Notes in Bioinformatics)},
  year = {2006},
  volume = {4034 LNCS},
  pages = {278-289},
  note = {cited By (since 1996)16; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@63ae193d
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@3afb26eb
	Through org.apache.xalan.xsltc.dom.DOMAdapter@3eb82c8d; Conference
	Code:67894},
  abbrev_source_title = {Lect. Notes Comput. Sci.},
  abstract = {Background: Test-driven development (TDD) and pair programming are
	software development practices popularized by eXtreme Programming
	methodology. The aim of the practices is to improve software quality.
	Objective: Provide an empirical evidence of the Impact of both practices
	on package dependencies playing a role of package level design quality
	indicators. Method: An experiment with a hundred and eighty eight
	MSc students from Wroclaw University of Technology, who developed
	finance-accounting system in different ways (CS - classic solo, TS
	- TDD solo, CP - classic pairs, TP - TDD pairs). Results: It appeared
	that package level design quality indicators (namely package dependencies
	in an object-oriented design) were not significantly affected by
	development method. Limitations: Generalization of the results is
	limited due to the fact that MSc students participated in the study.
	Conclusions: Previous research revealed that using test-driven development
	instead of classic (test-last) testing approach had statistically
	significant positive Impact on some class level software quality
	indicators (namely CBO and RFC metrics) in case of solo programmers
	as well as pairs. Combined results suggest that the positive Impact
	of test-driven development on software quality may be limited to
	class level. Â© Springer-Verlag Berlin Heidelberg 2006.},
  address = {Amsterdam},
  affiliation = {Institute of Applied Informatics, Wroclaw University of Technology,
	Wyb.Wyspianskiego 27, 50370 Wroclaw, Poland},
  correspondence_address1 = {Madeyski, L.; Institute of Applied Informatics, Wroclaw University
	of Technology, Wyb.Wyspianskiego 27, 50370 Wroclaw, Poland; email:
	Lech.Madeyski@pwr.wroc.pl},
  document_type = {Conference Paper},
  isbn = {3540346821; 9783540346821},
