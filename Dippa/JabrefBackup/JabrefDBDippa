@ARTICLE{Bussell200621,
  author = {Bussell, B.a and Taylor, S.b },
  title = {Software development as a collaborative writing project},
  journal = {Lecture Notes in Computer Science (including subseries Lecture Notes
	in Artificial Intelligence and Lecture Notes in Bioinformatics)},
  year = {2006},
  volume = {4044 LNCS},
  pages = {21-31},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@2c41a275
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@910c606
	Through org.apache.xalan.xsltc.dom.DOMAdapter@2c38b890; Conference
	Code:67905},
  abbrev_source_title = {Lect. Notes Comput. Sci.},
  abstract = {Software describes an imagined machine. To be software, the description
	must be executable, which means written so a computer can animate
	it. Non-executable descriptions (specifications, designs, &c.) are
	instrumental to this work; they are intermediate texts. We advance
	a model of software development as the collaborative writing of a
	series of descriptions. We propose the chief distinction of agile
	development to be the exclusion from this process of the human translation
	of intermediate texts. We distinguish supported and unsupported communication.
	We analyse the success of Extreme Programming in terms of avoiding
	unsupported communication and prioritising feedback from executable
	descriptions. We describe functional programming techniques to construct
	notations that allow programmers and users to collaborate writing
	executable system descriptions, collapsing distinctions between analysis,
	design, coding and testing. We describe a metric of code clarity,
	semantic density, which has been essential to the success of this
	work. We report the use of these techniques in the Pensions division
	of Britain's largest insurer, and its effect on the business. Â©
	Springer-Verlag Berlin Heidelberg 2006.},
  address = {Oulu},
  affiliation = {Norwich Union Life, 60 Wellington Way, York YO90 1LZ, United Kingdom;
	British APL Association, 81 South Hill Park, London NW3 2SS, United
	Kingdom},
  correspondence_address1 = {Bussell, B.; Norwich Union Life, 60 Wellington Way, York YO90 1LZ,
	United Kingdom; email: brian.bussell@norwich-union-life.co.uk},
  document_type = {Conference Paper},
  isbn = {3540350942; 9783540350941},
  issn = {03029743},
  keywords = {Code clarity; Collaborate writing; Functional programming techniques;
	Non-executable descriptions, Animation; Computer programming; Computer
	supported cooperative work; Logic design; Mathematical models; Project
	management; Semantics, Software engineering},
  language = {English},
  references = {Graves, R., (1996) The Greek Myths, p. 263. , London, Folio Soc; Sykes,
	M., Kx systems user meeting Vector, 21 (2). , http://www.vector.org.uk/archive/v212/kx212.htm,
	Global Markets Finance, Deutsche Bank, London; Fodor, J., (1983)
	The Modularity of Mind, , MIT Press, Boston; Wittgenstein, L., (1922)
	Logische-philosophische Abhandlung, , Cambridge; Wittgenstein, L.,
