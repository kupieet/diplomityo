@CONFERENCE{Dolan2013,
  author = {Dolan, B.a and Villalva, R.a and Munday, D.a and Gutmark, E.a and
	Zink, G.a and Ryon, J.b and Pack, S.b and Goeke, J.b },
  title = {Biofuel emissions of a multipoint staged low NOx combustion system
	at intermediate pressures},
  year = {2013},
  address = {San Jose, CA},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@6774ef4e
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@2e0b1ef2
	Through org.apache.xalan.xsltc.dom.DOMAdapter@5c1cb97b; Conference
	Code:99260},
  abbrev_source_title = {49th AIAA/ASME/SAE/ASEE Joint Propulsion Conf.},
  abstract = {The biofuel R-8 (a 50/50 mix of biofuel and Jet A) was tested in a
	low NOX combustor concept developed by United Technologies Aerospace
	Systems for NASA's Environmentally Responsible Aircraft program.
	The multipoint lean direct injection system employed substitutes
	five radially distributed rows of injectors for a single row of larger
	nozzles. The nozzles are grouped into three independently controlled
	fuel stages featuring differing air swirler and fuel injector design.
	The R-8 biofuel remains lit at a slightly lower overall fuel to air
	ratio (FAR) of 0.0054 compared to Jet A which extinguishes at a FAR
	of 0.0055. Emissions measurements for NOX, UHC, and CO are presented
	at different equivalence ratios, pressures, and staging configurations.
	OH* chemiluminescence images help explain the observed emissions
	trends. These images also demonstrate how the reaction zone changes
	drastically depending on the fuel stage configuration. The maximum
	NOx production using the pilot stage alone is measured when the local
	equivalence ratio (calculated from air and fuel flow through the
	pilot stage) is slightly above stoichiometric. In this pilot-only
	mode, there is very little difference in emissions between the R-8
	and Jet A fuels. In the two-stage configuration with constant equivalence
	ratio, increasing the relative amount of fuel sent to the second
	stage decreases the NOX production significantly. For these configurations
	there are large differences between R-8 and Jet A in that the biofuel
	produces more NOX emissions. In a three-stage mode a well distributed
	and low temperature reaction zone with low NOX production is demonstrated.
	The R-8 biofuel again shows more NOX emissions but the size of this
	difference is dependent on the fuel distribution.},
  affiliation = {University of Cincinnati, Cincinnati, OH, 45221, United States; United
	Technologies Aerospace Systems, Des Moines, IA, 50265, United States},
