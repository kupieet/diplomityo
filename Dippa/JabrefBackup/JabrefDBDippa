@CONFERENCE{Hourcade20101115,
  author = {Hourcade, J.P.a and Nguyen, C.M.b and Perry, K.B.a and Denburg, N.L.b
	},
  title = {PointAssist for older adults: Analyzing sub-movement characteristics
	to aid in pointing tasks},
  year = {2010},
  volume = {2},
  pages = {1115-1124},
  address = {Atlanta, GA},
  note = {cited By (since 1996)3; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@aa33acc
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@62692c88
	Through org.apache.xalan.xsltc.dom.DOMAdapter@66eac731; Conference
	Code:80836},
  abbrev_source_title = {Conf Hum Fact Comput Syst Proc},
  abstract = {Perceptual, cognitive and motor deficits cause many older adults to
	have difficulty conducting pointing tasks on computers. Many strategies
	have been discussed in the HCI community to aid older adults and
	others in pointing tasks. We present a different approach in PointAssist,
	software that aids in pointing tasks by analyzing the characteristics
	of sub-movements, detecting when users have difficulty pointing,
	and triggering a precision mode that slows the speed of the cursor
	in those cases. PointAssist is designed to help maintain pointing
	skills, runs as a background process working with existing software,
	is not vulnerable to clusters of targets or targets in the way, and
	does not modify the visual appearance or the feel of user interfaces.
	There is evidence from a prior study that PointAssist helps young
	children conduct pointing tasks. In this paper, we present a study
	evaluating PointAssist with twenty older adults (ages 66-88). The
	study participants benefited from greater accuracy when using PointAssist,
	when compared to using the "enhance pointer precision" option in
	Windows XP. In addition, we provide evidence of correlations between
	neuropsychological measures, pointing performance, and PointAssist
	detecting pointing difficulty. Â© 2010 ACM.},
  affiliation = {Department of Computer Science, University of Iowa, Iowa City, IA
	52242, United States; Department of Neurology, University of Iowa,
