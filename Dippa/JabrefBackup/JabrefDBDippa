@ARTICLE{NoAuthor200448,
  title = {Mitsubishi goes beyond MES to incorporate lean},
  journal = {Quality},
  year = {2004},
  volume = {43},
  pages = {48-49},
  number = {10},
  note = {cited By (since 1996)1},
  abbrev_source_title = {Qual},
  abstract = {Mitsubishi has installed Production IQ system for complete production
	monitoring and down-time tracking of all eight of its coil assembly
	lines. Production IQ is flexible and accomodates the need for wireless
	connections between system hardware and production equipment. The
	system could calculate and display a real-time production target,
	based on the cycle time of the process and broadcast real-time production
	performance to the factory floor. The Webview module of the Production
