@CONFERENCE{Davidsson2004269,
  author = {Davidsson, M. and Zheng, J. and Nagappan, N. and Williams, L. and
	Vouk, M.},
  title = {GERT: An empirical reliability estimation and testing feedback tool},
  year = {2004},
  pages = {269-280},
  address = {Saint-Malo},
  note = {cited By (since 1996)4; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@75f7a840
