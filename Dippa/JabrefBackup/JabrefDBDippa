@CONFERENCE{Dubinsky200611,
  author = {Dubinsky, Y.a and Hazzan, O.b and Talby, D.c and Keren, A.c},
  title = {System analysis and design in a large-scale software project: The
	case of transition to agile development},
  year = {2006},
  volume = {ISAS},
  pages = {11-18},
  address = {Paphos},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@45ddbddd
