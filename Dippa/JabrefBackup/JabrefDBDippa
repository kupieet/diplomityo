@CONFERENCE{Gousios2008129,
  author = {Gousios, G. and Kalliamvakou, E. and Spinellis, D.},
  title = {Measuring developer contribution from software repository data},
  year = {2008},
  pages = {129-132},
  address = {Leipzig},
  note = {cited By (since 1996)2; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@7f14dac9
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@3228034d
	Through org.apache.xalan.xsltc.dom.DOMAdapter@60f0cdf4; Conference
	Code:74376},
  abbrev_source_title = {Proc Int Conf Software Eng},
  abstract = {Apart from source code, software infrastructures supporting agile
	and distributed software projects contain traces of developer activity
	that does not directly affect the product itself but is important
	for the development process. We propose a model that, by combining
	traditional contribution metrics with data mined from software repositories,
	can deliver accurate developer contribution measurements. The model
	creates clusters of similar projects to extract weights that are
	then applied to the actions a developer performed on project assets
	to extract a combined measurement of the developer's contribution.
	We are currently implementing the model in the context of a software
	quality monitoring system while we are also validating its components
	by means of questionnaires. Copyright 2008 ACM.},
  affiliation = {Department of Management Science and Technology, Athens University
	of Economics and Business},
