@CONFERENCE{Rezaee2011121,
  author = {Rezaee, A.a and Rahmani, A.M.a and Adabi, S.b and Adabi, S.b },
  title = {A fuzzy algorithm for adaptive multilevel queue management with QoS
	feedback},
  year = {2011},
  pages = {121-127},
  address = {Istanbul},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@ac4c650
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@77700b Through
	org.apache.xalan.xsltc.dom.DOMAdapter@215ce26a; Conference Code:86619},
  abbrev_source_title = {Proc. Int. Conf. High Perform. Comput. Simul., HPCS},
  abstract = {Multilevel and multiclass queuing is very common in systems having
	QoS related concerns and one of the most common and well-know approaches
	is Weighted Fair Queuing (WFQ). In WFQ there are multiple queues
	and each one is dedicated for a distinct class of jobs or traffic
	and there is a weight value assigned to each queue to indicate the
	number of jobs that has to be picked from the queue in next round.
	This approach uses fixed predefined weight values tuned by system
	administrator with knowledge about system's characteristics. In this
	paper we extend WFQ approach by adding a fuzzy inference system and
	QoS measurement component. The proposed algorithm uses its easy-to-extend
	and agile knowledge base to configure the queue weights dynamically
	and autonomously in reaction to recent QoS metrics to achieve better
	performance and improved QoS. The Proposed method tailored to be
	used in virtual resource management environments especially in Grid
	services. To evaluate the performance of the proposed algorithm,
	simulation software used to compare it with well-known queue management
	approaches. The simulation results confirm the validity of the proposed
	algorithm. Â© 2011 IEEE.},
  affiliation = {Department of Computer Engineering, Science and Research Branch, Islamic
	Azad University, Tehran, Iran; Department of Computer Engineering,
	North Tehran Branch, Islamic Azad University, Tehran, Iran},
  art_number = {5999815},
  author_keywords = {Adaptive Dynamic Fuzzy Queue Management; Grid Services; Multilevel
