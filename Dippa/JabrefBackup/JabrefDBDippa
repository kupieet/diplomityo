@ARTICLE{Masood200624,
  author = {Masood, M.a and Mehdi, S.N.a and Omar, M.c and Reddy, P.R.b },
  title = {Effect of injection deky on performance and emissions in hydrogen-diesel
	dual fuel engine at different compression ratios},
  journal = {Journal of the Institution of Engineers (India): Mechanical Engineering
	Division},
  year = {2006},
  volume = {87},
  pages = {24-25},
  number = {JULY},
  note = {cited By (since 1996)6},
  abbrev_source_title = {J Inst Eng India Mech Eng Div},
  abstract = {Hydrogen is considered to be the fuel of future as it's use does not
	lead to CO, CO 2 hydrocarbon (HC) emission. It can he used in existing
	internal combustion engines, yielding unprecedented efficiencies
	and extremely low levels of exhaust pollution. Hydrogen can be used
	in conjunction with compact liquid fuels such as gasoline, alcohol
	or diesel provided each is stored separately. The drawback of lean
	operation with hydrocarbon fuels is a reduced power output. Lean
	operation of hydrocarbon engines has additional drawbacks. Lean mixtures
	are hard to ignite 1. Mixing hydrogen with other hydrocarbon fuels
	reduces all these drawbacks. Hydrogen's low ignition energy limit
	and high burning speed makes the hydrogen/ hydrocarbon mixture easier
	to ignite, reducing misfire and thereby improving emissions, performance
	and fuel economy. The engine used in the present study was a Kirloskar
	AV-1, single cylinder direct injection diesel engine. Diesel was
	injected through a nozzle of hole size 0.15mm. A CNG injector (to
	inject simultaneously hydrogen directly in to the cylinder) was also
	provided. The engine was coupled to a dc dynamometer. The engine
	was modified and provision was made to vary the compression ratio
	from 14.5 to 24.5. A computer interfaced piezo sensor, of range (2000
	psi) was used to note the pressure inside the cylinder. Special software
	was used to obtain combustion parameters related with pressure inside
	the cylinder. The hydrogen gas was inducted into the intake manifold
	and a thermal mass flow controller controlled its flow rate. The
	pressure of hydrogen which was at 150 bars inside the cylinder was
	reduced to atmospheric pressure before inducting it into the manifold.
	The airflow rate was measured using a laminar flow element. The speed
	of the engine was maintained constant by controlling the hydrogen
	gas mass flow rate. Emissions were measured using an advanced AVL
	five-gas analyzer. All the tests were conducted at a constant speed
	of 1400 rev/min, at various loads rangingfrom 20% to 100% of full
	load. The injection timing of diesel was varied from 40° BTDC to
	5° ATDC. During the experiments heating the air in the intake charge
	tank varied the inlet temperature of the air. The temperature was
	measured after the hydrogen was mixed with air in the inlet manifold.},
  affiliation = {Muffakahm Jha College of Engineering and Technology, Hyderabad 500
	034, India; Jawaharlal Nehru Technological University, Hyderabad,
	India; Mechanical Engineering Department, M J College of Engineering
	and Technology, Banjara Hills, Hyderabad 500 034, India},
  author_keywords = {Compression ratio; Diesel; Emissions; Hydrogen},
  coden = {JEMDA},
  correspondence_address1 = {Masood, M.; Muffakahm Jha College of Engineering and Technology, Hyderabad
	500 034, India},
  document_type = {Article},
  issn = {00203408},
  keywords = {Compression ratio; Single cylinder direct injection diesel engine,
	Compaction; Hydrogen; Mixing; Particulate emissions, Diesel fuels},
  language = {English},
  owner = {ekupiain},
  references = {Thring, R.H., (1989) Homogeneous Charge Compression Ignition (HCCI)
	Engines, , SAE Paper, no 892068; Liu, Z., Karim, G.A., (1995) The
	Ignition Delay Period in Dual Fuel Engines, , SAE Paper, no 950466;
	Varde, K.S., Frame, G.A., Hydrogen Aspiration in a Direct Injection
	Type Diesel Engine - It's Effect on Smoke and Other Engine Parameters'
	(1983) International Journal of Hydrogen Energy, 8 (7), pp. 549-555},
  source = {Scopus},
