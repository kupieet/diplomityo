@CONFERENCE{Racheva2008457,
  author = {Racheva, Z.a and Daneva, M.a and Buglione, L.b },
  title = {Complementing measurements and real options concepts to support interiteration
	decision-making in agile projects},
  year = {2008},
  pages = {457-464},
  address = {Parma},
  note = {cited By (since 1996)3; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@61ebb092
