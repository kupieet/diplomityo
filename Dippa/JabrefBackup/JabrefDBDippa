@CONFERENCE{Abdallah20131117,
  author = {Abdallah, A. and Hassan, R. and Azim, M.A.},
  title = {Quantified extreme scenario based design approach},
  year = {2013},
  pages = {1117-1122},
  address = {Coimbra},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@5e75b2d1
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@12ee7ca
	Through org.apache.xalan.xsltc.dom.DOMAdapter@2e737f61; Conference
	Code:96995},
  abbrev_source_title = {Proc ACM Symp Appl Computing},
  abstract = {Over the past few years, agile methods have recorded impressive results
	in the software engineering field basically in terms of business
	value and time to market. Similarly, software usability has been
	the major success factor for interactive systems. Agile usability
	engineering has been motivated as a booming solution to develop better
	usability in a cost-effective manner. eXtreme Scenario Based Design
	(XSBD) is one of the effective agile usability approaches. However,
	XSBD does not provide explicit metrics to quantify the software usability.
	In this paper, we propose the Quantified eXtreme Scenario Based Design
	(QXSBD) approach, which complements XSBD with a set of usability
	metrics collected from the agile architecture-centric approach and
	the Quality in Use Integrated Model (QUIM). Further, QXSBD specifies
	the subset of usability metrics that need to be assessed at the major
	milestones in agile process. This integration employs the Usability
	Critical Parameters Workshop (UCPW) to provide engineering practices
	defining the usability requirements and design goals. We demonstrate
	the QXSBD, process through customer request project case study. The
	obtained results have shown that usability issues have been reduced
	by about 30%. Noticeable end user satisfaction has been achieved
	while maintaining the project total cost. Copyright 2013 ACM.},
  affiliation = {Computer Science Department, Arab Academy for Science and Technology,
	Cairo, Egypt},
  author_keywords = {Agile; Critical parameters; Quantified evaluation; Software usability;
	User satisfaction},
  correspondence_address1 = {Computer Science Department, Arab Academy for Science and Technology,
	Cairo, Egypt},
  document_type = {Conference Paper},
  doi = {10.1145/2480362.2480572},
  isbn = {9781450316569},
  journal = {Proceedings of the ACM Symposium on Applied Computing},
  keywords = {Agile; Critical parameter; Quantified evaluations; Software usabilities;
	User satisfaction, Human computer interaction; Software engineering,
	Usability engineering},
  language = {English},
