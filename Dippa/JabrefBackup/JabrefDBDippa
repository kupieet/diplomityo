@CONFERENCE{Shrivastava2010671,
  author = {Shrivastava, D.P.a and Lakshminarayan, R.V.a and Sujatha, S.V.S.L.b
	},
  title = {New engineering technique of software development},
  year = {2010},
  pages = {671-676},
  address = {Cairo},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@778318ef
