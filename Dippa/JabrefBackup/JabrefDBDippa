@CONFERENCE{Baptista2008275,
  author = {Baptista, J.},
  title = {Agile documentation with uScrum},
  year = {2008},
  pages = {275-276},
  address = {Lisbon},
  note = {cited By (since 1996)2; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@ae4a176
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@28244f90
	Through org.apache.xalan.xsltc.dom.DOMAdapter@67e38b86; Conference
	Code:77867},
  abbrev_source_title = {SIGDOC - Proc. ACM Int. Conf. Des. Commun.},
  abstract = {uScrum (uncertainty Scrum) is an agile process developed by a small
	team at Altitude Software to manage the process of writing user documentation.
	uScrum manages uncertainty and the unknown, allowing writers to quickly
	react to changing conditions. uScrum uses orders of ignorance to
	understand the difficulty of tasks, allowing the team to effectively
	prioritize regular work together with difficult creative work. uScrum
	overbooks writers on iterative cycles called sprints, then lets the
	writers micro-manage their tasks to overcome obstacles. After each
	sprint the team decides what to publish and whether to proceed with
	unfinished work. Copyright 2008 ACM.},
  affiliation = {Altitude Software, Alameda FernÃ£o Lopes, 16 - 4 andar, 1495-136
	AlgÃ©s, Portugal},
