@CONFERENCE{Ji2011482,
  author = {Ji, F. and Sedano, T.},
  title = {Comparing extreme programming and Waterfall project results},
  year = {2011},
  pages = {482-486},
  address = {Waikiki, Honolulu, HI},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@6fcde039
