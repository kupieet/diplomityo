@CONFERENCE{Rueter199593,
  author = {Rueter, Curtis O. and Reif, Dirk L. and Menzies, William R. and Evans,
	James M.},
  title = {Measurement and enhanced monitoring of BTEX and VOC emissions from
	glycol dehydrators},
  year = {1995},
  pages = {93-108},
  address = {Houston, TX, USA},
  publisher = {Society of Petroleum Engineers (SPE), Richardson, TX, United States},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@5d57c51c
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@2d4de124
	Through org.apache.xalan.xsltc.dom.DOMAdapter@53f819b8; Conference
	Code:42901},
  abbrev_source_title = {SPE EPA Explor Prod Environ Conf},
  abstract = {The Clean Air Act Amendments of 1990 and activities by several states
	have resulted in increasing regulatory pressure on emissions of benzene,
	toluene, ethylbenzene, and xylenes (BTEX) and total volatile organic
	compounds (VOC) from glycol dehydration units. There are no standard
	and analytical methods established by regulatory agencies for determining
	emissions from glycol dehydrators. To address this lack of methods,
	the Gas Research Institute (GRI) began an effort to develop measurement
	methods and emission estimation techniques for glycol dehydrators.
	The results of the project showed that the GRI-GLYCalc should be
	used to initially screen a large number of units and that atmospheric
	rich/lean glycol measurements should be made to refine these initial
	estimates.},
  affiliation = {GRI},
  coden = {PSEPF},
  correspondence_address1 = {Rueter, Curtis O.; GRI},
