@CONFERENCE{Ball20021,
  author = {Ball, T. and Rajamani, S.K.},
  title = {The SLAM project: Debugging system software via static analysis},
  year = {2002},
  pages = {1-3},
  address = {Portland, OR},
  note = {cited By (since 1996)197; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@3ebd8790
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@d0f1f1e
	Through org.apache.xalan.xsltc.dom.DOMAdapter@5bc3f144; Conference
	Code:59591},
  abbrev_source_title = {Conf Rec Annu ACM Symp Princ Program Lang},
  abstract = {The goal of the SLAM project is to check whether or not a program
	obeys "API usage rules" that specify what it means to be a good client
	of an API. The SLAM toolkit statically analyzes a C program to determine
	whether or not it violates given usage rules. The toolkit has two
	unique aspects: It does not require the programmer to annotate the
	source program (invariants are inferred); it minimizes noise (false
	error messages) through a process known as "counterexample-driven
	refinement". SLAM exploits and extends results from program analysis,
	model checking and automated deduction. We have successfully applied
	the SLAM toolkit to Windows XP device drivers, to both validate behavior
	and find defects in their usage of kernel APIs.},
  coden = {CRLAD},
