@ARTICLE{Nelson201320,
  author = {Nelson, R.},
  title = {Hardware and software support multiple RE standards},
  journal = {EE: Evaluation Engineering},
  year = {2013},
  volume = {52},
  pages = {20-24},
  number = {3},
  note = {cited By (since 1996)1},
  abbrev_source_title = {EE Eval Engin},
  abstract = {Agilent offers hardware and software to address multistandard and
	radio (MSR) challenges. The company provides System Vue electronic
	system level design software and Signal Studio software for various
	signal generators. For signal analysis, Agilent offers 89600 Vector
	Signal Analysis (VSA) software for research and analysis and the
	X-Series measurement application for manufacturing. The System Vue
	SignalCombiner model combines multiple input signals with different
	radio formats and sample rates, different characterization frequencies,
	and different bandwidths into a single signal. Signal Studio software
	is a flexible suite of signal creation tools used with various Agilent
	signal sources. The LTTE FDD and LTT TDD Signal Studio applications
	allow users to import waveforms generated from 2G and 3G Signal Studio
	software and generate a single combined MSR. 89600 VSA software with
	multimeasurement capability analyzes multistandard devices in research
	and analysis and design validation.},
  coden = {EVENA},
