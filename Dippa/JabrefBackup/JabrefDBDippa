@ARTICLE{Spector2004176,
  author = {Spector, L.},
  title = {Why Is My Speedy PC S-l-o-w-i-n-g D-o-w-n?},
  journal = {PC World (San Francisco, CA)},
  year = {2004},
  volume = {22},
  pages = {176-177},
  number = {3},
  note = {cited By (since 1996)0},
  abbrev_source_title = {PC World (San Francisco CA)},
  abstract = {The possible reasons for a PC slowdown and ways to get the speed back
	are discussed. The running of too many applications and programs
	simultaneously on the PC is the major cause for a slow system. The
	use of new and upgraded software on an outdated hardware is another
	major reason behind a slow PC. Spywares which are activity monitoring
	programs and an overloaded Windows XP Prefetch folder are also common
	causes that slow down the PC.},
