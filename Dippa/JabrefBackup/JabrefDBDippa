@CONFERENCE{DuPlain2008,
  author = {DuPlain, R.a and Ransom, S.a and Demorest, P.a and Brandt, P.b and
	Ford, J.b and Shelton, A.L.b },
  title = {Launching GUPPI: the green bank ultimate pulsar processing instrument},
  year = {2008},
  volume = {7019},
  address = {Marseille},
  note = {cited By (since 1996)1; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@6b19e58a
