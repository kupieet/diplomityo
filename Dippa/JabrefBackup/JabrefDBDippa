@CONFERENCE{Niammuad2006874,
  author = {Niammuad, D.a and Nutpramoon, R.a and Fraisse, R.b },
  title = {THEOS data processing and its image quality},
  year = {2006},
  pages = {874-879},
  address = {Ulaanbaatar},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@75094f2e
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@362d9109
	Through org.apache.xalan.xsltc.dom.DOMAdapter@698d01a8; Conference
	Code:92278},
  abbrev_source_title = {Asian Assoc. Remote Sens. - Asian Conf. Remote Sens., ACRS},
  abstract = {Thailand Earth Observation System (THEOS) programme, developed by
	Geo-Informatics and Space Technology Development Agency (Public Organization)
	(GISTDA) under the Ministry of Science and Technology, with European
	Aeronautic Defence and Space Company - Astrium (EADS Astrium SAS),
	in France, as a prime contractor, has begun in July 2004. It is designed
	to fulfil the needs of using high resolution data, large swath width
	and frequent revisit time from satellite remote sensing to monitor
	and support the exploitation of resources for various applications
	such as cartography, agriculture, forestry, etc. The aim of this
	paper is to present the THEOS system, its mission & image performances,
	and ground segment data processing. THEOS satellite is a compact
	and agile satellite with two push-broom scanning optical instruments,
	providing worldwide imagery in Panchromatic and Multispectral modes,
	covering the Visible and Near Infrared spectrum. The Panchromatic
	instrument has a resolution of 2 metres and a swath width of 22 kilometres.
	The Multispectral instrument has a resolution of 15 metres and a
	swath width of 90 kilometres. Foreseen performances, geometrical
	image quality (Ground Sampling Distance, Modulation Transfer Function,â‹¯)
	and radiometric image quality (Signal-to-Noise Ratio, Compression,â‹¯)
	will be presented with their associated on ground processing (e.g.
	resampling, deconvolution, denoising). Data downlinked from the instrument
	will be transmitted to THEOS IGS (Image Ground Segment), on-ground
	data processing. The role of THEOS IGS is to provide standard and
	value added products from THEOS images corresponding to end users'
	needs. Its hardware and software enable the automatic processing
	of the transmitted image telemetry up to level 2A. The working stations
	of the Image Exploitation Facility provide high level tools to generate
	value added and precision products.},
  affiliation = {Geo-Infomatics and Space Technology Development Agency (GISTDA), 10900,
	Bangkok, Thailand; EADS Astrium, 31402, Toulouse, France},
