@CONFERENCE{Hodgkins2007194,
  author = {Hodgkins, P.a and Hohmann, L.b },
  title = {Agile program management: Lessons learned from the VeriSign managed
	security services team},
  year = {2007},
  pages = {194-199},
  address = {Washington, DC},
  note = {cited By (since 1996)1; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@2050a9f2
