@ARTICLE{NoAuthor2006284,
  title = {Automated test and data management},
  journal = {Microwave Journal},
  year = {2006},
  volume = {49},
  pages = {284-290},
  number = {5},
  note = {cited By (since 1996)1},
  abbrev_source_title = {Microwave J},
  abstract = {Spartan Automated Test and Data management has raised the bar in the
	test and evaluation of RF and microwave components and systems. Implementing
	Spartan in production facilitates lean manufacturing goals and will
	provide access to the data that can provide a competitive edge. The
	flexibility and power and Spartan allows the user to confidently
	outsource test automation to get the information contained within
	the data rather than the data collection. Modern test instruments
	and software development tools make it easier than ever to realize
	an automated measurement capability. Modern automated test systems
	generate a significant amount of data and within this data resides
	valuable information beyond a simple pass or fail decision. Spartan
	has been implemented with a bar code reader for data input, which
	minimizes the chances of error, and a touch screen monitor to keep
	the test area clear of computer accessories.},
  coden = {MCWJA},
