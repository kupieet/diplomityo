@CONFERENCE{Bonner2010592,
  author = {Bonner, N.A.a and Teng, J.T.C.b and Nerur, S.b },
  title = {The perceived advantage of agile development methodologies by software
	professionals: Testing an innovation-theoretic model},
  year = {2010},
  volume = {1},
  pages = {592-603},
  address = {Lima},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@145abf74
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@51cf631c
	Through org.apache.xalan.xsltc.dom.DOMAdapter@673ed3a; Conference
	Code:92980},
  abbrev_source_title = {Amer. Conf. Inf. Sys., AMCIS},
  abstract = {Proponents of agile processes claim that agile practices result in
	higher quality software while allowing the flexibility to respond
	to evolving user requirements. Yet, to the best of our knowledge,
	no empirical study has really confirmed that benefits accrue to those
	who use agile processes. Grounded in the agility and diffusion of
	innovations literature, this research introduces a measure of process
	agility within the software development domain and relates it to
	constructs previously employed in the innovation literature. For
	agile methods, our study has provided empirical support for the proposal
	that agile development methods lead to developers' beliefs that they
	are less complex, more compatible and provide increased benefits.
	Since developers believe that agility leads to increased benefits,
	they will be more likely to accept agile methods. For practitioners,
	this study provides valuable insights into the underlying factors
	that influence a developer's beliefs about agile methods.},
  affiliation = {University of Mary Hardin-Baylor, United States; University of Texas
	at Arlington, United States},
  author_keywords = {Agility; Methodology; Process innovation; Software development; Technology
	acceptance},
