@CONFERENCE{Tõnnisson2006169,
  author = {Tõnnisson, T.},
  title = {Portable FTIR spectrometer},
  year = {2006},
  pages = {169-170},
  address = {Tallinn},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@7c841c90
