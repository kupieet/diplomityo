@CONFERENCE{Sheng2011715,
  author = {Sheng, Z. and Li, Y.},
  title = {Development of computer controlled loading system of helicopter manipulating
	booster},
  year = {2011},
  pages = {715-720},
  address = {Beijing},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@310c9334
