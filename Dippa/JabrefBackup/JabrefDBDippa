@ARTICLE{Padayachee200775,
  author = {Padayachee, K.a and Eloff, J.H.P.b },
  title = {Enhancing optimistic access controls with usage control},
  journal = {Lecture Notes in Computer Science (including subseries Lecture Notes
	in Artificial Intelligence and Lecture Notes in Bioinformatics)},
  year = {2007},
  volume = {4657 LNCS},
  pages = {75-82},
  note = {cited By (since 1996)1; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@33092356
