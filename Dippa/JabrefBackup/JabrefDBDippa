@ARTICLE{McCaffery200724,
  author = {McCaffery, F.a and Taylor, P.S.b and Coleman, G.c },
  title = {Adept: A unified assessment method for small software companies},
  journal = {IEEE Software},
  year = {2007},
  volume = {24},
  pages = {24-31},
  number = {1},
  note = {cited By (since 1996)22},
  abbrev_source_title = {IEEE Software},
  abstract = {Software process assessments are typically the first step to commencing
	software process improvement. Small software companies find that
	many assessment methods are linked to plan-driven improvement models
	and can be expensive in terms of the resources required. The authors
	support these claims with statistics from the Irish software industry
	and present a lightweight assessment method called Adept. Adept unifies
	an existing plan-driven assessment method and an adapted, risk-based
	Agility/Discipline Assessment method. Adept has a low resource overhead
	and does not dictate either a plan-driven or agile process improvement
	model, making it an attractive assessment method for small software
	companies. Â© 2007 IEEE.},
  affiliation = {Lero - The Irish Software Engineering Research Centre, Regional Development
