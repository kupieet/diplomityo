@CONFERENCE{Talby2006100,
  author = {Talby, D.a and Hazzan, O.b and Dubinsky, Y.c and Keren, A.a },
  title = {Reflections on reflection in agile software development},
  year = {2006},
  volume = {2006},
  pages = {100-110},
  address = {Minneapolis, MN},
  note = {cited By (since 1996)5; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@17f30c24
