@CONFERENCE{Dekkers2002551,
  author = {Dekkers, C.a and McQuaid, P.b },
  title = {The Dangers of Using Measurement to (Mis)Manage: Measuring the Software
	Process},
  year = {2002},
  pages = {551-560},
  address = {Denver, CO},
  note = {cited By (since 1996)1; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@34b6bce9
