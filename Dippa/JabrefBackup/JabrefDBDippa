@ARTICLE{KrÃ¡l201010,
  author = {KrÃ¡l, J.a b and Å½emliÄ�ka, M.a},
  title = {SOA worlds},
  journal = {Communications in Computer and Information Science},
  year = {2010},
  volume = {110 CCIS},
  pages = {10-19},
  number = {PART 2},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@473b5bc7
