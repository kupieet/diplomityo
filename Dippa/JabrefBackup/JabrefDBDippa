@CONFERENCE{Kusumasari2011,
  author = {Kusumasari, T.F.a and Supriana, I.b and Surendro, K.b and Sastramihardja,
	H.b },
  title = {Collaboration model of software development},
  year = {2011},
  address = {Bandung},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@7b28eb8f
