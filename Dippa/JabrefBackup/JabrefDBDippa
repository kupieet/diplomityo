@CONFERENCE{Mangalaraj2007383,
  author = {Mangalaraj, G.A.a and Bhadauria, V.S.b },
  title = {Pair programming: Effects of trust on software quality},
  year = {2007},
  volume = {1},
  pages = {383-390},
  address = {Keystone, CO},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@5a580b00
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@4a3a5b56
	Through org.apache.xalan.xsltc.dom.DOMAdapter@3d42912c; Conference
	Code:92969},
  abbrev_source_title = {Assoc. Inf. Sys. - Amer. Conf. Inf. Sys., AMCIS: Reaching New Heights},
  abstract = {Trust is increasingly becoming an important area of research in software
	development with the proliferation of agile methodologies. In this
	study, we propose to investigate the effect of trust on the performance
	of a pair programming team. High levels of trust in the competence
	of the fellow member are hypothesized to increase the perceived effort
	applied in software development. This relationship is moderated by
	the characteristics of the software development task. Perceived effort
	applied at the task is hypothesized to mediate the relationship between
	trust and the team performance which is measured in terms of the
	solution quality. We propose to use a laboratory experiment to test
	our model. Limitations are discussed and future research directions
	are suggested.},
  affiliation = {Western Illinois University, United States; University of Texas Arlington,
	United States},
