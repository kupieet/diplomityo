@ARTICLE{Berghel200523,
  author = {Berghel, H.a b c d and Hoelzer, D.e f g },
  title = {Pernicious ports},
  journal = {Communications of the ACM},
  year = {2005},
  volume = {48},
  pages = {23-30},
  number = {12},
  note = {cited By (since 1996)4},
  abbrev_source_title = {Commun ACM},
  abstract = {The importance of identifying and defending against port-related vulnerabilities
	to computer software is discussed. Taking preventive measures against
	potential insecurities is highly important to prevent any loss due
	to such vulnerability problems. Common port vulnerabilities are grouped
	into Windows-centric, Unix-centric, and operating system-independent.
	The problem with WTS is that RDP has been shown to be vulnerable
	to denial-of-service attacks by malformed packets up to and including
	Service Pack 2 of Windows XP. TCP port 22 is used by the Secure Shell
	service. A service designed for security is listed in out set of
	ports to watch carefully and this service is a great target for attackers.
	If a proxy is not configured correctly, it will allow anyone on the
	Internet to connect with it and use it to connect anywhere else.
	Continuous oversight of the firewalls and routers are required to
	protect computer software against different vulnerabilities.},
  affiliation = {College of Engineering; University of Canterbury, New Zealand; Center
	for Cybermedia Research; National Identity Theft and Financial Fraud
	Research and Operations Center, University of Nevada, Las Vegas,
	United States; Internet Forensics Lab., CCR; SANS Institute; CyberDefense},
  coden = {CACMA},
  correspondence_address1 = {Berghel, H.; College of Engineering},
  document_type = {Review},
  doi = {10.1145/1101779.1101798},
  issn = {00010782},
  keywords = {Ports; Secure shell service; Windows XP; Windows-centric, Computer
	operating systems; Computer software; Product design; Product development;
	Security of data; Security systems, Software engineering},
