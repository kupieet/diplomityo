@CONFERENCE{Brown19972332,
  author = {Brown, Russell G. and Brost, Randy C.},
  title = {3-d Modular gripper design tool},
  year = {1997},
  volume = {3},
  pages = {2332-2339},
  address = {Albuquerque, NM, USA},
  publisher = {IEEE, Piscataway, NJ, United States},
  note = {cited By (since 1996)5; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@7b35920d
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@7a07d2ac
	Through org.apache.xalan.xsltc.dom.DOMAdapter@179858cb; Conference
	Code:46971},
  abbrev_source_title = {Proc IEEE Int Conf Rob Autom},
  abstract = {Modular fixturing kits are sets of components used for flexible, rapid
	construction of fixtures. A modular vise is a parallel-jaw vise,
	each jaw of which is a modular fixture plate with a regular grid
	of precisely positioned holes. To fixture a part, one places pins
	in some of the holes so that when the vise is closed, the part is
	reliably located and completely constrained. The modular vise concept
	can be adapted easily to the design of modular parallel-jaw grippers
	for robots. By attaching a grid-plate to each jaw of a parallel-jaw
	gripper, we gain the ability to easily construct high-quality grasps
	for a wide variety of parts from a standard set of hardware. Wallack
	and Canny developed an algorithm for planning planar grasp con figurations
	for the modular vise. In this paper, we expand this work to produce
	a 3-d fixture/gripper design tool. We describe several analyses we
	have added to the planar algorithm, including a 3-d grasp quality
	metric based on force information, 3-d geometric loading analysis,
	and inter-gripper interference analysis. Finally, we describe two
	applications of our code. One of these is an internal application
	at Sandia, while the other shows a potential use of our code for
	designing part of an agile assembly line.},
  affiliation = {Sandia Natl Lab, Albuquerque, United States},
  coden = {PIIAE},
  correspondence_address1 = {Brown, Russell G.; Sandia Natl Lab, Albuquerque, United States},
  document_type = {Conference Paper},
  issn = {10504729},
  journal = {Proceedings - IEEE International Conference on Robotics and Automation},
  keywords = {Algorithms; Computer aided design; Computer software; Fixtures (tooling);
	Loading, Modular gripper design tool, Grippers},
  language = {English},
  source = {Scopus},
  sponsors = {IEEE},
