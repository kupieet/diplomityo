@CONFERENCE{Rajkumar2012,
  author = {Rajkumar, E.R.a and Safaie, J.b and Gupta, R.a and Pattnaik, D.a
	and Abrishamimoghaddam, H.b and Grebe, R.b and Wallois, F.b },
  title = {Development of an autonomic portable single-board computer based
	high resolution NIRS device for microcirculation analysis},
  year = {2012},
  pages = {3235-3238},
  address = {San Diego, CA},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@3e0f26f8
