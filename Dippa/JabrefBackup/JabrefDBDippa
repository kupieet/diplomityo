@CONFERENCE{Bugalho200931,
  author = {Bugalho, R.a and Carriço, B.a and Ferreira, C.S.a and Ferreira,
	M.a and Moura, R.a and Ortigáo, C.a and Pinheiro, J.a and Rodrigues,
	P.a and Silva, J.C.a and Trindade, A.a and Varela, J.b },
  title = {Avalanche photodiodes for high-resolution pet imaging systems},
  year = {2009},
  pages = {31-38},
  address = {Porto},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@39f3e63b
