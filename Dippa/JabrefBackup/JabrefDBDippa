@ARTICLE{Steers200566,
  author = {Steers, K.},
  title = {Five-minute fixes},
  journal = {PC World (San Francisco, CA)},
  year = {2005},
  volume = {23},
  pages = {66-72},
  number = {1},
  note = {cited By (since 1996)0},
  abbrev_source_title = {PC World (San Francisco CA)},
  abstract = {Some of the lightning-fast fixes for a locked-up PC for solving the
	hardware and software problems in just a few minutes are discussed.
	A printer self-test is performed using the diagnostic utility, built
	on the device that checks its basic functionality. It is suggested
	to change the channel that a Wi-Fi network uses by going into the
	setup program for the router or access point for reducing interference
	from the wireless phones. It is also suggested to use the drying
	process with a fan or hair dryer, in case of spilling of liquid on
	the keyboard.},
