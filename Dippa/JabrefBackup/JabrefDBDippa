@CONFERENCE{Malmqvist2010107,
  author = {Malmqvist, R.a and Samuelsson, C.a and Rantakari, P.b and VÃ¤hÃ¤-HeikkilÃ¤,
	T.b and Smith, D.c and Varis, J.b and Baggen, R.d },
  title = {RF MEMS and MMIC based reconfigurable matching networks for adaptive
	multi-band RF front-ends},
  year = {2010},
  pages = {107-110},
  address = {Aveiro},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@51e23b72
