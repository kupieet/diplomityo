@ARTICLE{Piyabunditkul20121,
  author = {Piyabunditkul, C.a and Lichter, H.b and Anwar, T.c and Methawachananont,
	A.a and Krootkaew, C.a and Krisanathamakul, T.a },
  title = {Design and evaluation of a CMMI conformant light-weight project management
	approach},
  journal = {International Journal of Digital Content Technology and its Applications},
  year = {2012},
  volume = {6},
  pages = {1-10},
  number = {21},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Int. J. Digit. Content Technol. Appl.},
  abstract = {CMMI is one of the well-known and accepted maturity models that many
	software organizations have implemented for its quality processes
	which are expected to bring a good quality for their software products.
	However, traditional software process models become too heavy-weight
	to be deployed. The aim of this research is to design the Light-Weight
	Project Management (LWPM) approach to implement CMMI by mapping between
	CMMI goals and Agile-Scrum based on defined artifacts and to indicate
	the differences in applying LWPM and the traditional Waterfall model.
	Our approach focuses on the Project Management category which composes
	Project Planning (PP), Project Monitoring and Control (PMC) and Integrated
	Project Management (IPM). In order to compare both models we collected
	relevant data by using questionnaire and also the dedicated tool
	SPIALS (Software Process Improvement Adaptive Learning System).},
  affiliation = {TGGS-NECTEC/NSTDA, Thailand; RWTH Aachen University, Germany; Faculty
	of Computer Science and Information Systems, Universiti Teknologi
