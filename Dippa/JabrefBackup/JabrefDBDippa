  author = {Wang, Z.a and Chandra, R.b and Moscibroda, T.b and Gefflaut, A.a
	and De Baynast, A.a and Bahl, V.b },
  title = {An agile radio framework for unmanaged wireless environments},
  year = {2009},
  pages = {333-334},
  address = {New Orleans, LA},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@74b3cab5
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@47c16cee
	Through org.apache.xalan.xsltc.dom.DOMAdapter@6bd6a20f; Conference
	Code:78547},
  abbrev_source_title = {Proc. Int. Symp. Mobile Ad Hoc Networking Comput.},
  abstract = {The proposed demonstration is based on commodity 802.11 wireless cards
	and a low cost 2.4GHz sniffing device and shows how current WLAN
	based networks can benefit from spectrum awareness and dynamic access
	to the assigned band. The demonstrator presents a solution to problems
	in current wireless networks, like inefficient radio spectrum usage
	and limited ability to withstand interferences. The presented spectrum-aware
	radio management framework flexibly negotiates transmission parameters
	based on spectrum usage and application requirements and opportunistically
	utilizes available bandwidth. A key feature of this demonstrator
	is the fact that it breaks the traditional fixed channel bandwidth
	limitation and enables dynamic bandwidth allocation according to
	the needs of applications. It continuously monitors the assigned
	spectrum, tracks application behaviour, and dynamically adapts to
	the radio environment, such as interference and competition. Furthermore,
	a cross-media roaming mechanism is provided in the framework to support
	seamless handovers within and between radio technologies. Our enhancements
	are transparent to upper layer applications and will automatically
	benefit any software using network resources. The presented demonstrator
	uses wireless video streaming in a typical household scenario to
	illustrate the benefits without changes to the multimedia applications.
	Copyright 2009 ACM.},
  affiliation = {European Microsoft Innovation Center, Aachen, Germany; Mircosoft Research,
	Redmond, WA, United States},
  correspondence_address1 = {Wang, Z.; European Microsoft Innovation Center, Aachen, Germany; email:
	zhouwang@microsoft.com},
  document_type = {Conference Paper},
  doi = {10.1145/1530748.1530800},
  isbn = {9781605585314},
  journal = {Proceedings of the International Symposium on Mobile Ad Hoc Networking
	and Computing (MobiHoc)},
  keywords = {Application requirements; Available bandwidth; Channel bandwidth;
	Cross-media; Dynamic bandwidth allocation; Hand over; Key feature;
