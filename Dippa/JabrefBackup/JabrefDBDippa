@CONFERENCE{Curry2007318,
  author = {Curry, P.A.a and Burden, J.b and Lundy, G.A.c },
  title = {Next generation automatic test system (NGATS) update},
  year = {2007},
  pages = {318-322},
  address = {Anaheim, CA},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@3d89791e
