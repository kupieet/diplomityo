@CONFERENCE{Bowyer2006691,
  author = {Bowyer, J. and Hughes, J.},
  title = {Assessing undergraduate experience of continuous integration and
	test-driven development},
  year = {2006},
  volume = {2006},
  pages = {691-694},
  address = {Shanghai},
  note = {cited By (since 1996)9; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@274ee1fe
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@2bd6fedb
	Through org.apache.xalan.xsltc.dom.DOMAdapter@2ff5f259; Conference
	Code:69469},
  abbrev_source_title = {Proc Int Conf Software Eng},
  abstract = {A number of agile practices are included in software engineering curricula,
	including test-driven development. Continuous integration often is
	not included, despite it becoming increasingly common in industry
	to code, test, and integrate at the same time. This paper describes
	a study whereby software engineering undergraduates were given a
	short intensive experience of test-driven development with continuous
	integration using an environment that imitated a typical industrial
	circumstance. Assessment was made of students' agile experience rather
	than of project deliverables, using a novel set of process measures
	that examined students' participation and performance in agile testing.
	Results showed good participation by student pairs, and clear understanding
	of agile processes and configuration management. Future work will
	investigate automation of the assessment of continuous integration
	and configuration management server data.},
  affiliation = {Applied Computing, University of Dundee, Dundee DD1 4HN, United Kingdom},
