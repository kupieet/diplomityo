@ARTICLE{Martinovic2012218,
  author = {Martinovic, G.a and Balen, J.a and Cukic, B.b },
  title = {Performance evaluation of recent windows operating systems},
  journal = {Journal of Universal Computer Science},
  year = {2012},
  volume = {18},
  pages = {218-263},
  number = {2},
  note = {cited By (since 1996)1},
  abbrev_source_title = {J. Univers. Comput. Sci.},
  abstract = {The primary goal of most OSs (Operating Systems) is the efficient
	use of computer systems software and hardware resources. Since Windows
	OSs are most widely used OS for personal computers, they need to
	satisfy needs of all different kind of computer systems users. In
	comparison with Windows XP, new versions of the Windows OS; namely
	Windows Vista and Windows 7, introduce a number of new features and
	enhancements. Furthermore, performance improvement was imposed as
	one of the key design goals for both Windows Vista and Windows 7.
	This paper presents a performance evaluation of three latest versions
	of the Microsoft OS for personal computers; namely Windows XP, Windows
	Vista and Windows 7. OS performance measurement is done by means
	of a set of benchmark applications in the controlled environment.
	To ensure accurate, reliable and repeatable performance measurement
	results, we have created a performance measurement process and a
	performance evaluation model. Special emphasis is placed on evaluation
	areas with the greatest impact on the performance: CPU scheduling,
	memory management, graphic subsystem management, hard disk drive
	management and network performance. To determine the Windows OSs
	performance in different environments, performance measurement is
	done in three experiments. Experimental results indicate that Windows
	Vista and Windows 7 have several performance improvements on the
	stand-alone high-end computer system, but Windows XP outperforms
	Windows Vista and Windows 7 on the stand-alone low-end computer system.
	Furthermore, on network computer system Windows Vista and Windows
	7 show network performance improvements mostly for the traffic with
	medium-sized packets. Â© J.UCS.},
  affiliation = {Faculty of Electrical Engineering, Josip Juraj Strossmayer University
	of Osijek, Croatia; Lane Department of Computer Science and Electrical
	Engineering, West Virginia University, Morgantown, WV, United States},
  author_keywords = {Benchmark; Operating System; Performance Evaluation; Performance Measurement;
	Windows 7; Windows Vista; Windows XP},
  correspondence_address1 = {Martinovic, G.; Faculty of Electrical Engineering, Josip Juraj Strossmayer
	University of Osijek, Croatia; email: goran.martinovic@etfos.hr},
