@ARTICLE{Ramírez2008393,
  author = {Ramírez, F.a b and Pontón, M.b and Sancho, S.a b b and Suárez,
	A.a b b },
  title = {Phase-noise analysis of injection-locked oscillators and analog frequency
	dividers},
  journal = {IEEE Transactions on Microwave Theory and Techniques},
  year = {2008},
  volume = {56},
  pages = {393-407},
  number = {2},
  note = {cited By (since 1996)23},
  abbrev_source_title = {IEEE Trans. Microwave Theory Tech.},
  abstract = {In-depth investigation of the phase-noise behavior of injection-locked
	oscillators and analog frequency dividers is presented. An analytical
	formulation has been obtained, which allows a better understanding
	of the shape of the output phase-noise spectrum of these circuits.
	The simplicity of this formulation is also helpful for circuit design.
	Approximate expressions for the corner frequencies of the spectrum
	are determined, identifying the most influential magnitudes and deriving
	design criteria. In particular, a technique has been developed to
	shift the frequency of the first corner of the phase-noise spectrum,
	up to which the output phase noise follows the input one. The expressions
	for the corner frequencies can be introduced in either in-house or
	commercial harmonic-balance software, thus allowing an agile design,
	as no separate phase-noise analysis is required. The validity of
	the analytical techniques is verified with the conversion-matrix
	approach and with measurements using two field-effect-transistor-based
	circuits: a 4.9-GHz injection-locked oscillator and a frequency divider
	by 2 with 9.8-GHz input frequency. © 2008 IEEE.},
  affiliation = {IEEE; Communications Engineering Department, University of Cantabria,
	ETSIIT, 39005 Santander, Spain},
  author_keywords = {Circuit design; Frequency divider; Harmonic balance; Injection-locked
	oscillator; Phase noise; Stability analysis},
  coden = {IETMA},
  correspondence_address1 = {Ramírez, F.; Communications Engineering Department, University of
	Cantabria, ETSIIT, 39005 Santander, Spain; email: ramirezf@unican.es},
  document_type = {Article},
