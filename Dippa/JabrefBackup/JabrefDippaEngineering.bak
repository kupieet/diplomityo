  isbn = {0769525741; 9780769525747},
  issn = {10816011},
  journal = {Proceedings - IEEE Symposium on Security and Privacy},
  keywords = {Malicious services; Malware; Virtual machines, Codes (symbols); Computer
	operating systems; Computer software; Computer systems; Security
	systems; Telecommunication services, Virtual reality},
  language = {English},
  references = {(2000) VMware Virtual Machine Technology. Technical Report, , VMware,
	Inc., September; AMD platform for trustworthy computing (2003) WinHEC,
	, September; (2004) Advanced Configuration and Power Interface Specification,
	, http://www.acpi.info/DOWNLOADS/ACPIspec30.pdf, September; (2005)
	Installing VMware Tools, , http://www.vmware.com/support/ws5/doc/new_guest_tools_ws.html,
	Novemeber; (2006) ttylinux, , http://www.minimalinux.org/ttylinux/showpage.php?pid=1;
