@CONFERENCE{Diaferio2011335,
  author = {Diaferio, M.a and Foti, D.a and Mongelli, M.a and Giannoccaro, N.I.b
	and Andersen, P.c },
  title = {Operational modal analysis of a historic tower in Bari},
  year = {2011},
  volume = {4},
  pages = {335-342},
  address = {Jacksonville, FL},
  note = {cited By (since 1996)3; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@79b5a821
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@5314cf4
	Through org.apache.xalan.xsltc.dom.DOMAdapter@67b329d5; Conference
	Code:85096},
  __markedentry = {},
  abbrev_source_title = {Conf. Proc. Soc. Exp. Mech. Ser.},
  abstract = {In this paper, the latest identification techniques both in the time
	and in the frequency domains, are applied to the data obtained from
	the dynamic monitoring of the reinforced concrete tower of the Provincial
	Administration Building of Bari (Italy). The tower, dating back to
	the thirties of the 20th century and about 60 m tall, is not only
	a typical example of the fascist architectural style, but it is an
