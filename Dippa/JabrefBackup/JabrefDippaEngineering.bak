@ARTICLE{Waller201214,
  author = {Waller, R.},
  title = {Embedded instrumentation-Closing the T\&M gap for breakthrough technologies},
  journal = {Electronics World},
  year = {2012},
  volume = {118},
  pages = {14-17},
  number = {1915},
  note = {cited By (since 1996)0},
  __markedentry = {},
  abbrev_source_title = {Electron World},
  abstract = {Embedded instrumentation can be defined as the logic designed into
	semiconductors, used for design validation, test, debug, yield analysis
	and other T&M (test and measurement) activities. Embedded instrumentation
	has become critically important in the industry as intrusive external
	or modular testers which rely on probing a device or a circuit board
	are losing their effectiveness. Software-based embedded instrumentation
	which tests from the inside out rather than from the outside in is
	much more cost-effective, efficient, agile and better suited to a
	wide range of modern emerging computer and communications technologies.
	The concept of a tools platform or a unified tool environment is
	most appropriate for embedded instrumentation since different tools
	may be needed at any given point in the life-cycle of a chip, circuit
	board or system. Intel has developed embedded instrumentation methodology
	called Interconnect Built In Self Test (IBIST). Because it is embedded
	into chips and onto circuit boards with those chips, IBIST can test
	and measure all lanes on all buses concurrently.},
  correspondence_address1 = {Waller, R.},
  document_type = {Article},
  issn = {13654675},
  keywords = {Breakthrough technology; Circuit boards; Communications technology;
	Design validation; Software-based; Test and measurement; Yield analysis,
	Electronics engineering, Electrical engineering},
  language = {English},
  source = {Scopus},
  stage2status = {Exclude},
  url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84864242107&partnerID=40&md5=54bbbb3b9d3bb69876ee053583af2856}
}

