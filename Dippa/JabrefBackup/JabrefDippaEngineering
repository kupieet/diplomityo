@CONFERENCE{Dulipovici2006292,
  author = {Dulipovici, M. and Robillard, P.N.},
  title = {Vector-based method for measuring and comparing software requirements
	implementation},
  year = {2006},
  pages = {292-297},
  address = {Puerto Vallarta},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@1a537451
