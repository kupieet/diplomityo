@CONFERENCE{Jian20117375,
  author = {Jian, N.Y. and Noda, Y. and Terashima, K.},
  title = {Simulator building for agile control design of shipboard crane and
	its application to operational training},
  year = {2011},
  volume = {18},
  number = {PART 1},
  pages = {7375-7383},
  address = {Milano},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@756beccb
