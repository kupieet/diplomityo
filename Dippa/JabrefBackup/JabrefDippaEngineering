@CONFERENCE{Papadopoulou2007134,
  author = {Papadopoulou, T. and Mousavi, A.},
  title = {Dynamic job-shop lean scheduling and CONWIP shop-floor control using
	software agents},
  year = {2007},
  number = {528 CP},
  pages = {134-141},
  address = {Durham},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@6aba7138
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@765f4a45
	Through org.apache.xalan.xsltc.dom.DOMAdapter@6a0534b9; Conference
	Code:76267},
  abbrev_source_title = {IET Conf Publ},
  abstract = {With leanness finding its birthplace in the post war automobile industry
	and with complex functional shop-floor configurations posing strong
	impediments to the application of critical lean scheduling and shop-floor
	control enablers, the lean paradigm was adopted and implemented almost
	exclusively in repetitive production systems utilising flow-shop
	layout configurations. This research employs state-of- the-art agent-based
	simulation to apply lean scheduling principles and techniques on
	the shop-floor control of job-shops with functional layouts. The
	modelled system is a dynamic job-shop environment with stochastic
	order arrivals and processing times employing a variety of dispatching
	rules. The system's performance under push and global pull constant
	work-in-progress (CONWIP) control is studied in terms of a number
	of time, due date and work-in-progress related performance metrics.},
  affiliation = {Brunel University, UK, School of Engineering and Design, Uxbridge,
	Middlesex, UB8 3PH, United Kingdom},
  author_keywords = {CONWIP; Leanness; Multi agent systems; Performance modelling; Scheduling},
