  issn = {2010376X},
  keywords = {Agile Methodologies; Change management; Common problems; Metarequirements;
	Model change; Pro actives; Project schedules; Software engineering
	process; Meta-requirements, Impact resistance; Software engineering;
	Engineering; Technology, Management; Software engineering},
