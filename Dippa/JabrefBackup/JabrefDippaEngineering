@CONFERENCE{Lai2004665,
  author = {Lai, M.K.a and Freeman, W.G.a and Yankowich, P.R.b and Bryant, J.D.c
	and Walterscheid, P.c },
  title = {High pressure spray diagnostics facility for development and evaluation
	of aero-engine atomizer and swirler assemblies},
  year = {2004},
  volume = {1},
  pages = {665-671},
  address = {Vienna},
  note = {cited By (since 1996)1; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@1a295e5c
