@CONFERENCE{Nagappan200662,
  author = {Nagappan, N. and Ball, T. and Murphy, B.},
  title = {Using historical in-process and product metrics for early estimation
	of software failures},
  year = {2006},
  pages = {62-71},
  address = {Raleigh, NC},
  note = {cited By (since 1996)20; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@7dcd6cd9
