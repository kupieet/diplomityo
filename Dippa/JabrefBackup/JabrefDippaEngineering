@ARTICLE{Parker200436,
  author = {Parker, K.},
  title = {Lean purists and technologists declare truce in light of U.S. manufacturing
	crisis},
  journal = {MSI},
  year = {2004},
  volume = {22},
  pages = {36},
  number = {4},
  note = {cited By (since 1996)0},
  abbrev_source_title = {MSI},
  abstract = {The growth in lean projects by US manufacturers, for which the manufacturers
	themselves are responsible, as a way to reduce lead times, increase
	productivity, and slash inventories is discussed. The term lean includes
	the production systems that schedule based on actual customer demand
	rather than batch work orders. The continuation of global production
	overcapacity and availability of cheap labor in China can't be addressed
	solely through cost-cutting measures, but rather must be offset by
	US manufacturers offering greater value to their customers. Although
	US manufacturers are increasingly committed to lean methodologies,
	there must be some limit to the focus on labor costs that have moved
	so much manufacturing to Mexico and China.},
  document_type = {Short Survey},
