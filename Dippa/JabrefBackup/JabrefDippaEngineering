  title = {Avalanche photodiodes for high-resolution pet imaging systems},
  year = {2009},
  pages = {31-38},
  address = {Porto},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@39f3e63b
