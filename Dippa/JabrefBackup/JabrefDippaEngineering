@ARTICLE{NoAuthor200869,
  title = {From the typewriter to the PC and beyond},
  journal = {Electronic Design},
  year = {2008},
  volume = {56},
  pages = {69-73},
  number = {12},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Electron. Des.},
  abstract = {The Korean company Celluon has developed CL800BT, which uses a red
	laser diode, to display functioning QWERTY keyboard image of 240
	by 105 mm on a non-reflective and opaque flat surfaces. The keyboard
	can be seen in ambient light of 1000-5000 lux and provide keystroke
	distance of 2 mm. The system has a red-laser projector module of
	dimensions 93 by 39 by 37 mm and 109g of weight. The CL800BT is capable
	to make an interface with PC or portable device through the RS-232C
	protocol. The CL800BT is compatible to Windows 2000/XP/Vista/ Mobile
	Pocket PC & Smart Phone, Palm OS, BlackBerry, and Symbian. The touch-it
	software utility from Chessware SA displays a keyboard on the computer
	monitor to provide a working touchscreen. The touch-it software can
	support multi-monitor and alpha-blending, which allow user to load
	libraries in the touch-it environment.},
