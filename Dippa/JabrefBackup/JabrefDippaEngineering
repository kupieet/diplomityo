@CONFERENCE{Wilson2004,
  author = {Wilson, J.L.},
  title = {Using program management for successful lean transformation},
  year = {2004},
  volume = {2004},
  address = {Los Angeles, CA},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@480e891c
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@324bb239
	Through org.apache.xalan.xsltc.dom.DOMAdapter@7a1d5fb; Conference
	Code:69405},
  abbrev_source_title = {Annu. Lean Manage. Solut. Conf. Proc.},
  abstract = {Have you decided to "Lean" your organization but are not sure where
	to start? Eliminating waste from your organization's processes can
	produce tremendous savings that you can pass along to your customers.
	Knowing where to start is not always an easy task. Program management
	provides a good starting point to help establish and work toward
	your strategic objectives, and Lean provides the improvement tools
	to help you achieve that. This presentation will provide an overview
	of program management methodologies and how they apply to successfully
	implement Lean transformation. In order to have a successful Lean
	transformation, the entire scope of the program needs to be evaluated
	from the beginning. Program management starts at the beginning and
	creates an assessment of the entire organization. These methodologies
	help align an organization's overall health and manage the entire
	life cycle of the transformation. They (methodologies) help analyze
	your program's risks, determine costs and returns on investments,
	and develop your action plan and milestones. Developing a proper
	plan can guide you through the improvement process. Outlining the
	reasons for needing improvement should be the first step in addressing
	why you need to incorporate Lean into your processes. Your reasons
	should be commensurate with the strategic goals of the organization.
	This is known as the conception phase, where the idea begins. Do
	you know what your expected benefits are? What are the goals? Is
	it worth the investment? Is it feasible? Do you have adequate resources?
	Next is the definition phase; this is where your planning is critical
	to success. Reviewing your current baseline provides an idea of where
	you are. Using a "current-state map" is an excellent visual cue in
	determining how to prioritize your projects. During this phase a
	team should be identified with a sponsor and proper resources. There
	should be a charter or statement of work that outlines: the problem
	statement; project scope; background information; key measurements
	and targets; risks and benefits; and any assumptions that could affect
	project performance. The project manager or team leader will need
	to outline roles and responsibilities for all team members. Team
	member selection is vital to change because one naysayer can impede
	the progress or reduce enthusiasm. The start phase is where the rubber
	meets the road. Based on discovery in the definition phase, teams
	will measure and analyze data that is gathered. During this phase,
	numerous Lean and Six Sigma tools can be used, such as: value stream
	mapping and analysis; pareto charts; fishbone diagrams; control charts;
	failure mode effects analysis (FMEA); and regression and variance
	analyses. Identification of process capabilities, areas of waste
	and variability, and bottlenecks will become obvious in this phase.
	The performance or improvement phase requires commitment and buy-in
	from every person in the organization. Conducting experiments and
	developing action plans is where the actual improvements are seen,
	whether it is through a pull or kanban system; doing setup reduction
	on a production line; or using preventative maintenance on equipment.
	But the key to success is outlining schedules for completion and
	tracking the implementation. Using a software tool such as Microsoft
	Project is an excellent way to visually track progress and identify
	causes of delays or variances that may impact schedule performance
	and implementation of improvement activities. The last phase, the
	control phase, is never really "over," rather it is a work in progress.
	The program manager captures metrics and measurement of the improvements
	made and compares to the baseline. Sustaining the newly improved
	processes is the toughest part because it involves accepting change
	as the "new way of doing business." Program management tries to expect
	the unexpected and anticipate the issues before they arise or get
	out of hand. It customizes practical experience and expertise and
	applies it to each individual organization. Using these streamlined
	processes reduces costs and cycle time. Program management also increases
	productivity and quality of work. By using program management you
	can assure the success of the Lean transformation throughout the
	enterprise. The process is continual and always in need of review;
	it should become a natural part of "institutionalizing" process change
	in the organization.},
  affiliation = {Robbins-Gioia, LLC, Warner Robins, GA, United States},
  correspondence_address1 = {Wilson, J.L.; Robbins-Gioia, LLC, Warner Robins, GA, United States},
  document_type = {Conference Paper},
  journal = {4th Annual Lean Management Solutions Conference 2004, Conference
	Proceedings},
  keywords = {Cost reduction; Production control; Productivity; Risk assessment;
	Scheduling, Failure mode effects analysis (FMEA); Lean transformation;
	Program management, Industrial management},
  language = {English},
  page_count = {5},
  source = {Scopus},
  sponsors = {East Carolina University; Missouri Enterprise; North Carolina State
	University; Operations Concepts, Inc; University of Kentucky},
  stage2status = {Exclude},
  url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-34047147154&partnerID=40&md5=9471c08fb05e859296f53f3e4d7eb651}
}

@ARTICLE{Wong200714,
  author = {Wong, K.},
  title = {2600 B.C. lean manufacturing},
  journal = {Cadalyst},
  year = {2007},
  volume = {24},
  pages = {14-17},
  number = {7},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Cadalyst},
  abstract = {Jean-Pierre Houdin has presented Khufu pyramid construction theory.
	Houdin's theory is based on an internal ramp spiraling to the top
	of the pyramid. Houdin and a multidisciplinary team of 15 engineers,
	designers, and developers build a 3D geometric model of the Great
	Pyramid, Pharaoh Khufu's tomb using CATIA, the industrial design
	software. He used the dimensions of the pyramid and the 2D plans
	from the measurements made Gilles Dormion, French architect, to evaluate
	the relationship between different elements of the pyramid. He tested
	the counterweight system used by ancient builders to transport the
	granite beams to the site of the King's Chamber using a combination
	of geometric and physical modeling. Using DELMIA, Dassault's software
	for organizing factory-floor operations, the team evaluated amount
	of human labor used and time taken to complete the project. The digital
	simulation indicates that the project to 20 years to accomplish.},
