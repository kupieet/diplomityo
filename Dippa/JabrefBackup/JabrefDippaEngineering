@CONFERENCE{Fisk2002285,
  author = {Fisk, M.a and Varghese, G.b },
  title = {Agile and Scalable Analysis of Network Events},
  year = {2002},
  pages = {285-290},
  address = {Marseille},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@288186cd
