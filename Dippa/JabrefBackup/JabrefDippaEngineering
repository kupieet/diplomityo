@ARTICLE{vonBromssen1996316,
  author = {von Bromssen, Bernt},
  title = {Hot straightening procedures for high-strength structural steels
	should be qualified by testing strength characteristics},
  journal = {Welding in the World, Le Soudage Dans Le Monde},
  year = {1996},
  volume = {37},
  pages = {316-331},
  number = {6},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Weld World Soudage Monde},
  abstract = {Hot straightening tests were performed on high strength structural
	steels: TMCP steels produced by SSAB Tunnplat AB (Domex 390 YP and
	Domex 690 XP) as well as quenched and tempered steels produced by
	SSAB Oxelosund AB (Weldox 700 and 960). It is shown that modest heating
	(below 600Â°C) for a short period (less than 10 min) did not affect
	either the impact toughness or the fatigue strength. Workpieces substantially
	deformed (greater than 5%) and then straightened often have a lower
	fatigue strength than the original fatigue strength of the material.
	It is recommended that the temperature is continuously measured with
	thermoelements. The geometry (straightness) after straightening can
	have a significant effect on fatigue life. Gas heating or induction
	heating are recommended. The surface smoothness can be slightly improved
	by glass peening with an intensity of 0.10 Almen A, which also increases
	the fatigue life, sometimes by a factor 10 or more. The particular
	case of hot straightening of front axles for reducing tyre wear on
	trucks is discussed. A PC program has been established to simplify
	the straightening procedure.},
  affiliation = {IVF, Sweden},
  coden = {WDWRA},
  correspondence_address1 = {von Bromssen, Bernt; IVFSweden},
  document_type = {Article},
  issn = {00432288},
