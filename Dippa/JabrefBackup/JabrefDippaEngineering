@ARTICLE{2002a,
  title = {CAD/ CAM, controls and factory auotmation},
  journal = {Manufacturing Engineering},
  year = {2002},
  volume = {129},
  number = {2},
  note = {cited By (since 1996)0},
  abbrev_source_title = {Manuf Eng},
  abstract = {Manufacturers are increasingly using CAD/CAM, controls and factory
	automation, including advanced software and automation solutions,
	for enhancing productivity. They are adopting more advanced CAD/CAM
	packages, open controls, and greater networking solutions, including
	real-time supply chain systems over the Internet to optimize their
	productivity. CAM packages offers automatic feature recognition (AFR),
	machining directly from solids, and high-speed machining techniques.
	Lean manufacturing and productivity improvements are increasingly
	being implemented to reduce waste and achieve the high productivity.
	The adoption of advanced networks through Internet connections are
	making remote diagnostics and machine tool monitoring more efficient.
	CAD/CAM software supports major enhancements, including automatic
	CAD file repair that checks IGES files for errors and repairs them.
	WorkNC V.15 software package provides functions for mold, die, and
	tooling to deploy through manufacturing processes.},
