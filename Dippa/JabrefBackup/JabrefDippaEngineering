@CONFERENCE{Hashish1994887,
  author = {Hashish, Mohamed},
  title = {Agile machining with an intelligent abrasive-waterjet system},
  year = {1994},
  volume = {68-2},
  pages = {887-891},
  address = {Chicago, IL, USA},
  publisher = {ASME, New York, NY, United States},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@5e1f9550
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@6b83e776
	Through org.apache.xalan.xsltc.dom.DOMAdapter@20d12b1f; Conference
	Code:42353},
  abbrev_source_title = {ASME Prod Eng Div Publ PED},
  abstract = {This paper presents a new abrasive-waterjet nozzle system with several
	new features for automated and intelligent machining towards agile
	manufacturing. The jet-forming components, i.e., the waterjet orifice
	and the abrasive mixing tube, were combined in a cartridge assembly
	that can be handled as a `tool' and aligned off-line. A high-pressure
	face seal was developed to allow the cartridge to be automatically
	or manually connected to the nozzle body. High-pressure water, abrasives,
	flushing lines, vacuum assist lines, air control lines, and sensors
	are all attached to the nozzle body on the end effector. An abrasive
	flow rate sensor was developed and integrated with an automated abrasive
	feed valve for closed-loop control of abrasive flow rate. An orifice
	health sensor was developed to monitor the quality of the waterjet.
	Parameter selection software was also developed to select the optimal
	nozzle cartridge and other operating parameters. Multiple and automatic
	machining operations were demonstrated with the new nozzle.},
  affiliation = {QUEST Integrated, Inc, Kent, United States},
  coden = {ASMDD},
  correspondence_address1 = {Hashish, Mohamed; QUEST Integrated, Inc, Kent, United States},
