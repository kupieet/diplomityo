@CONFERENCE{Payne2008233,
  author = {Payne, B.D. and Carbone, M. and Sharif, M. and Lee, W.},
  title = {Lares: An architecture for secure active monitoring using virtualization},
  year = {2008},
  pages = {233-247},
  address = {Oakland, CA},
  note = {cited By (since 1996)76; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@1a84a4b0
