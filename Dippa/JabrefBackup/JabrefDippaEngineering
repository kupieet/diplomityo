@CONFERENCE{Bilirgen1999261,
  author = {Bilirgen, H. and Levy, E.K.},
  title = {Design of flow mixing devices for pneumatic conveying systems},
  year = {1999},
  editor = {Anon},
  volume = {397 II},
  pages = {261-270},
  address = {San Francisco, CA, USA},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@9c1adc2
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@a76a08f
	Through org.apache.xalan.xsltc.dom.DOMAdapter@722711a6; Conference
	Code:49917},
  abbrev_source_title = {ASME Pressure Vessels Piping Div Publ PVP},
  abstract = {The presence of solids flow maldistributions is a common occurrence
	in lean phase pneumatic conveying. As the gas and particles change
	direction in an elbow, the particles stratify into a narrow localized
	stream, forming what is referred to as a particle rope. The presence
	of roping can cause operating and maintenance problems, including
	pipe erosion, difficulties in measuring solids flow rate and difficulties
	in controlling the flow distributions in complex piping networks.
	This paper describes the application of computational fluid dynamics
	(CFD) software to develop and design flow mixing devices for pneumatic
	conveying applications. A combined numerical and experimental study
	was conducted to investigate mixing with devices installed immediately
	downstream of a 90 degree pipe bend. Coal particles with a mean diameter
	of 59 Î¼m were transported with air in a pipeline which has a vertical
	run of 3.4 m and an inside diameter of 0.154 m. Numerical simulations
	were carried out using the CFX-4.2 code developed by AEA Technology.
	Particle motions were modeled using the Lagrangian tracking scheme.
	In order to verify these results, a reflective fiber optic probe
	was employed to measure local particle velocities and concentrations.
	The comparison of predicted and measured particle velocities and
	concentrations showed that CFD simulations gave good agreement with
	the measurements.},
  affiliation = {Energy Research Center, Lehigh University, Bethlehem, PA 18015, United
	States},
  coden = {AMPPD},
  correspondence_address1 = {Bilirgen, H.; Energy Research Center, Lehigh University, Bethlehem,
	PA 18015, United States},
  document_type = {Article},
  issn = {0277027X},
