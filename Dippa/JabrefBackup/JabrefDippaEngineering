@CONFERENCE{Chen2001975,
  author = {Chen, B. and Braun, J.E.},
  title = {Simple Rule-Based Methods for Fault Detection and Diagnostics Applied
	to Packaged Air Conditioners},
  year = {2001},
  pages = {975-984},
  address = {Atlanta, GA},
  note = {cited By (since 1996)2; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@31de114f
