@CONFERENCE{Lubarsky2008,
  author = {Lubarsky, E.a and Cross, C.N.b and Cutright, J.T.b and Zinn, B.T.a
	and Knaus, D.c and Magari, P.c },
  title = {Novel carbureted flame holder for improved afterburner stability},
  year = {2008},
  address = {Reno, NV},
  note = {cited By (since 1996)0; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@7aba1ad0
	; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@50368ca2
	Through org.apache.xalan.xsltc.dom.DOMAdapter@2d4b08d2; Conference
	Code:82092},
  abbrev_source_title = {AIAA Aerosp. Sci. Meet. Exhib.},
  abstract = {A novel flame holder that makes use of a carbureted fuel injection
	system (CFIS) has recently been developed. Initial testing of the
	CFIS flame holder was performed under conditions similar to those
	present in a modern gas turbine engine thrust augmentor in order
	to determine the stability limits of the flame holder design. Six
	different fuel injection configurations were tested in an effort
	to identify the key factors contributing to stable operation. Flame
	stabilization was achieved only at lean operating conditions for
	five of the fuel injection configurations, while the use of extension
	tubes at the carburetor channel exits in the sixth configuration
	expanded the stability limits to include richer operating conditions.
	Two of the fuel injection configurations were modeled extensively
	using computational fluid dynamics (CFD) software. In addition, a
	Phase Doppler Particle Analysis (PDPA) was performed during the testing
	of one flame holder configuration to measure fuel droplet size emanating
	from the CFIS exits and into the wake. Stability was found to be
	highly dependent on fuel spray penetration into the main air stream
	and fuel distribution in the wake. Future efforts will focus on optimizing
	fuel placement to further improve stability limits. Copyright Â©
	2008 by the American Institute of Aeronautics and Astronautics, Inc.},
  affiliation = {School of Aerospace Engineering, Georgia Institute of Technology,
