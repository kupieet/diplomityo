@ARTICLE{Dye198532,
  author = {Dye, Anthony},
  title = {. . . NEW APPROACH TO COMBUSTION ANALYSIS.},
  journal = {Automotive Engineer (London)},
  year = {1985},
  volume = {10},
  pages = {32-35},
  number = {1},
  note = {cited By (since 1996)1},
  abbrev_source_title = {Automot Eng (London)},
  abstract = {Meeting the challenge of forthcoming emissions legislation in Europe
	must focus the attention of combustion systems engineers on the need
	to improve the consistency of the combustion process on a cycle to
	cycle basis. Dilution of the air/fuel charge mainly to reduce NO//x
	emissions either by using exhaust gas (exhaust gas recirculation
	method) or with excess air (lean-burn method) exacerbates the problem
	of intercyclic variation especially at road-load engine operating
	conditions. A new hardware/software package (EPICAS) has been developed
	as an analytical tool to meet the needs of combustion systems engineers
	in evaluating the effects on combustion of their own design improvements.
	This study has revealed the existence of combustion system performance
	differentials of a high order including: half to almost four fifths
	in the level of intercyclic variation as measured by 10 key combustion
	parameters.},
  affiliation = {Epicam Ltd, Epicam Ltd},
  coden = {EUEND},
  correspondence_address1 = {Dye, Anthony; Epicam Ltd, Epicam Ltd},
