@CONFERENCE{Rico2008,
  author = {Rico, D.F.},
  title = {Effects of agile methods on website quality for electronic commerce},
  year = {2008},
  address = {Big Island, HI},
  note = {cited By (since 1996)1; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@652fbd2e
